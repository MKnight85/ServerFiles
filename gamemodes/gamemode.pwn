COMMAND:goto(playerid, params[]) {
	if(PlayerInfo[playerid][pAdminlevel] > 0) {
	    if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
		new user;
		if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /goto [playerid]");
		if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
		if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
		if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
		if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
		if(user == playerid) return SendClientMessage(playerid, COLOR_GREY, "You cannot teleport to yourself.");
		new Float:gX,
			Float:gY,
			Float:gZ,
			interior,
			vw,
			msgAdm[47];

		GetPlayerPos(user, gX, gY, gZ);
		vw = GetPlayerVirtualWorld(user);
		interior = GetPlayerInterior(user);
		SetPlayerPosEx(playerid, 5, gX, gY + 1.5, gZ, 0.0, interior, vw);
		format(msgAdm, sizeof(msgAdm), "*** You teleported to %s.", GetNameWithSpace(user));
		SendClientMessage(playerid, COLOR_WHITE, msgAdm);
	} else {
		SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}

	return 1;
}

COMMAND:approveplayer(playerid, params[])
{
    if(PlayerInfo[playerid][pAdminlevel] > 0 || PlayerInfo[playerid][pModLevel] > 0)
	{
	    if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
		new username[MAX_PLAYER_NAME],
		   	reason[100],
		    cpNumber;

	    if(sscanf(params, "s[24]", username)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /approveplayer [Firstname_Lastname]");
	    if(!IsValidPlayerName(username)) return SendClientMessage(playerid, COLOR_GREY, "You have to enter a username according to the Firstname_Lastname format.");
	    if(GetPlayerID(username) != -1) return SendClientMessage(playerid, COLOR_GREY, "That player is currently connected to the server. They must be offline.");
		new query[100];
		mysql_format(sqlHandle, query, sizeof(query), "SELECT `activated`, `ip` FROM `samp_users` WHERE `user` = '%e'", username);
		mysql_function_query(sqlHandle, query, true, "OnCommandApproveAccount", "issi", playerid, username, reason, cpNumber);
	}
	else
	{
	    SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}

	return 1;
}

COMMAND:kick(playerid, params[]) {
	if(PlayerInfo[playerid][pAdminlevel] > 0 || PlayerInfo[playerid][pModLevel] > 0) {
	    if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
		new user,
			reason[128];

	    if(sscanf(params, "us[128]", user, reason)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /kick [playerid] [reason]");
	    if(user == INVALID_PLAYER_ID) sscanf(params, "is[128]", user, reason);
		if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	    if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
	    if(PlayerInfo[user][pAdminlevel] == 5) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this commnd on that player.");
		if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
		PlayAudioStreamForPlayer(user, "http://www.losangelesrp.org/sound/itisyourtime.mp3");
	    KickLog(user, PlayerInfo[playerid][pName], reason);
	    KickDelay(user);
	    if(IsPlayerConnected(user)) {
		    new msg[410];
		    if(PlayerInfo[playerid][pAdminlevel] > 0)
			{
   				format(msg, sizeof(msg), "AdmCmd: %s was kicked by %s, Reason:[%s]", GetNameWithSpace(user), GetNameWithSpace(playerid), reason);
			}
			else
			{
				format(msg, sizeof(msg), "AdmCmd: %s was kicked by %s, Reason:[%s]", GetNameWithSpace(user), GetNameWithSpace(playerid), reason);
			}

			SendClientMessageToAll(COLOR_LIGHTRED, msg);
		}
	} else {
	    SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}

	return 1;
}

ALTCOMMAND:ajail->adminjail;
COMMAND:adminjail(playerid, params[]) {
	if(PlayerInfo[playerid][pAdminlevel] > 0) {
	    if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
		new user,
			cpNumber,
			reason[100];

	    if(sscanf(params, "uis[100]", user, cpNumber, reason)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /adminjail [playerid] [pickups] [reason]");
	    if(user == INVALID_PLAYER_ID) sscanf(params, "iis[100]", user, cpNumber, reason);
		if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	    if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
	    if(PlayerInfo[user][pAdminlevel] == 5) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this commnd on that player.");
		if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	    if(GetPVarInt(user, "AdminJailed") != 0) return SendClientMessage(playerid, COLOR_GREY, "Player is already admin jailed.");
		if(cpNumber <= 0) return SendClientMessage(playerid, COLOR_GREY, "You have to set an amount of pickups to pass above zero.");
	    new query[275];
		format(query, sizeof(query), "AdmCmd: %s was admin jailed by %s, Reason:[%s]", GetNameWithSpace(user), PlayerInfo[playerid][pName], reason);
		SendClientMessageToAll(COLOR_LIGHTRED, query);
	    SetPVarInt(user, "Muted", 1);
		SetPVarInt(user, "AdminJailed", 1);
        SetPlayerPosEx(user, 5, 939.8325, -1251.1356, 781.2216, 0, 25, user + 1);
		PlayerInfo[user][pAJCPs] = cpNumber;
		SetPlayerTime(user, 12, 0);
		CreateNewAdminJailPickup(user);
		format(query, sizeof(query), "You have been admin jailed to enter %i pickup(s) by %s, Reason:[%s]", cpNumber, PlayerInfo[playerid][pName], reason);
		SendClientMessage(user, COLOR_WHITE, query);
		format(query, sizeof(query), "AdmJail: You have %i pickup(s) left to enter before being released (Use /refreshjail when no star is shown).", cpNumber);
		SendClientMessage(user, COLOR_LIGHTRED, query);
		mysql_format(sqlHandle, query, sizeof(query), "INSERT INTO `samp_jails` (`user`, `reason`, `checkpoints`, `admin`) VALUES ('%e', '%e', %i, '%e')", PlayerInfo[user][pName], reason, cpNumber, PlayerInfo[playerid][pName]);
		mysql_function_query(sqlHandle, query, false, "", "");
	} else {
	    SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}

	return 1;
}

