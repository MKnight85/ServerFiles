COMMAND:setfaction(playerid, params[])
{
	if(PlayerInfo[playerid][pAdminlevel] > 0)
	{
	    if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
		format(gVar3000, sizeof(gVar3000), "%s", EOS);
	    for(new i = 1; i != MAX_FACTIONS; i++)
		{
	        if(!isnull(FactionInfo[i][fName]))
			{
		        if(isnull(gVar3000))
				{
		            format(gVar3000, sizeof(gVar3000), "%s", FactionInfo[i][fName]);
		        }
				else
				{
					format(gVar3000, sizeof(gVar3000), "%s\n%s", gVar3000, FactionInfo[i][fName]);
				}
			}
	    }

	    if(isnull(gVar3000))
		{
     		format(gVar3000, sizeof(gVar3000), "Create New Faction");
     		//SendClientMessage(playerid, COLOR_LIGHTRED, "Do NOT create any new factions, don't reset factions. (RESET THEM MANUALLY)");
        }
		else
		{
			format(gVar3000, sizeof(gVar3000), "%s\nCreate New Faction", gVar3000);
			//SendClientMessage(playerid, COLOR_LIGHTRED, "Do NOT create any new factions, don't reset factions. (RESET THEM MANUALLY)");
		}

		ShowPlayerDialog(playerid, DIALOG_SETFACTION, DIALOG_STYLE_LIST, "{FF9900}Faction Manager", gVar3000, "Choose", "Cancel");
		format(gVar3000, sizeof(gVar3000), "%s", EOS);
	}
 	else if(PlayerInfo[playerid][pFaction] != 0 && (FactionInfo[PlayerInfo[playerid][pFaction]][fLeaderrank] > PlayerInfo[playerid][pRank] - 5))
	{
 	    if(PlayerInfo[playerid][pFaction] == 1 && PlayerInfo[playerid][pRank] < 16) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
        if(PlayerInfo[playerid][pFaction] == 2 && PlayerInfo[playerid][pRank] < 12) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
		ShowPlayerDialog(playerid, DIALOG_SETFACTION_LEADER, DIALOG_STYLE_LIST, "{FF9900}Faction Manager", "Invite New Member\nView/Edit Existing Members\nEdit MOTD\nEdit Ranknames\nToggle Faction Chat", "Choose", "Cancel");
	}
	else
	{
		SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}

	return 1;
}

COMMAND:spawncar(playerid, params[]) {
	if(PlayerInfo[playerid][pAdminlevel] > 0) {
	    if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
		new model,
			color1,
			color2,
			vehicle,
			Float:sX,
			Float:sY,
			Float:sZ,
			Float:sA,
			msgAdm[120],
			msgPlayer[88];

		if(sscanf(params, "iii", model, color1, color2)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /spawncar [Model] [Color 1] [Color 2]");
		if(model < 400 || model > 611) return SendClientMessage(playerid, COLOR_GREY, "Invalid vehicle model (Submitted vehicle model does not exist).");
		if(vehicleData[model - 400][blacklisted] != 0) return SendClientMessage(playerid, COLOR_GREY, "Invalid vehicle model (Submitted vehicle model is blacklisted).");
		if(color1 < 0 || color1 > 300 || color2 < 0 || color2 > 300) return SendClientMessage(playerid, COLOR_GREY, "Invalid vehicle color (Submitted vehicle color does not exist).");

		/* --- Retrieving Player-Data --- */

		GetPlayerPos(playerid, sX, sY, sZ);
		GetPlayerFacingAngle(playerid, sA);

		/* --- Creating vehicle + Locking vehicle --- */

		vehicle = CreateVehicle(model, sX, sY, sZ, sA, color1, color2, -1);
		LinkVehicleToInterior(vehicle, GetPlayerInterior(playerid));
		SetVehicleVirtualWorld(vehicle, GetPlayerVirtualWorld(playerid));
		SetVehicleNumberPlate(vehicle, "ADMIN");
		SetVehicleParamsEx(vehicle, 1, 0, 0, 0, 0, 0, 0);
		if(DoesVehicleObtainHelmets(vehicle)) {
			spawncarlabel[vehicle] = CreateDynamic3DTextLabel("Administrative Vehicle", COLOR_GOLD, 0, 0, 0.35, 50, INVALID_PLAYER_ID, vehicle);
		} else {
			spawncarlabel[vehicle] = CreateDynamic3DTextLabel("Administrative Vehicle", COLOR_GOLD, 0, 0, 1, 50, INVALID_PLAYER_ID, vehicle);
		}

		if(IsAnEngineCar(vehicle)) {
			FuelTimerVar[vehicle] = SetTimerEx("FuelTimer", GM_FUELTIMER, true, "d", vehicle);
		}

		/* --- Setting Vehicle-Data --- */

		VehicleInfo[vehicle][vFuel] = vehicleData[model - 400][tank];
		VehicleInfo[vehicle][vX] = sX;
		VehicleInfo[vehicle][vY] = sY;
		VehicleInfo[vehicle][vZ] = sZ;
		VehicleInfo[vehicle][vA] = sA;
		VehicleInfo[vehicle][vFaction] = 0;
		VehicleInfo[vehicle][vColor1] = color1;
	 	VehicleInfo[vehicle][vColor2] = color2;
  		VehicleInfo[vehicle][vHealth] = 1000;
		VehicleInfo[vehicle][vType] = VEHICLE_ADMIN;
		VehicleInfo[vehicle][vModifications][8] = 1086;
		format(VehicleInfo[vehicle][vOwner], MAX_PLAYER_NAME, "%s", PlayerInfo[playerid][pName]);
		format(VehicleInfo[vehicle][vPlate], 10, "%s", "ADMIN");

		/* --- Setting player into vehicle --- */

		PutPlayerInVehicle(playerid, vehicle, 0);

		/* --- Messaging + Logging --- */

		format(msgAdm, sizeof(msgAdm), "*** %s %s spawned Vehicle Model %i (Vehicle: %s, VehicleID %i).", GetAdminRank(playerid), GetNameWithSpace(playerid), model, vehicleData[model - 400][typeName], vehicle);
		format(msgPlayer, sizeof(msgPlayer), "Spawned Vehicle Model %i (VehicleID %i) - Use /destroycar to despawn the vehicle.", model, vehicle);
		SendClientMessage(playerid, COLOR_WHITE, msgPlayer);
		AdminMessageLog(msgAdm, playerid, "Spawn Vehicle");
	} else {
		SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}

	return 1;
}

COMMAND:check(playerid, params[]) {
	if(PlayerInfo[playerid][pAdminlevel] > 0 || PlayerInfo[playerid][pModLevel] > 0){
	    if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
	    new user;
		if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /check [playerid]");
		if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
		if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
		if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
		if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
		new lineStr[128],
			Float:X,
			Float:Y,
			Float:Z,
			Float:health,
			Float:armour,
			level = floatround(floatsqroot(PlayerInfo[user][pScore]), floatround_floor),
		    nextLevel = floatround(floatpower(level + 1, 2));

		/* --- Header --- */

		format(lineStr, sizeof(lineStr), "Statistics of %s:", GetNameWithSpace(user));
		SendClientMessage(playerid, COLOR_LIGHTGREEN, lineStr);

		/* --- Line 1 --- */

		format(lineStr, sizeof(lineStr), "Money (Client-side): [$%i], Money (Server-side): [$%i], Bank: [$%i], Savings: [$%i], Age: [%i]", GetPlayerMoney(user), PlayerInfo[user][pMoney], PlayerInfo[user][pBank], PlayerInfo[user][pSavings], PlayerInfo[user][pAge]);
		SendClientMessage(playerid, COLOR_GREY, lineStr);

		/* --- Line 2 --- */
		
	        switch(PlayerInfo[user][pGender])
			{
	            case 1:
				{
    				format(lineStr, sizeof(lineStr), "Gender: [Male], Level: [%i], Level Up: [%i / %i], Virtual World: [%i], Interior: [%i], Adminrank: [%i], Moderator Rank: [%i]", level, PlayerInfo[user][pScore], nextLevel, GetPlayerVirtualWorld(user), GetPlayerInterior(user), PlayerInfo[user][pAdminlevel], PlayerInfo[user][pModLevel]);
				}
				case 2:
				{

					format(lineStr, sizeof(lineStr), "Gender: [Female], Level: [%i], Level Up: [%i / %i], Virtual World: [%i], Interior: [%i], Adminrank: [%i], Moderator Rank: [%i]", level, PlayerInfo[user][pScore], nextLevel, GetPlayerVirtualWorld(user), GetPlayerInterior(user), PlayerInfo[user][pAdminlevel], PlayerInfo[user][pModLevel]);
				}
				default:
				{
                    format(lineStr, sizeof(lineStr), "Gender: [Female], Level: [%i], Level Up: [%i / %i], Virtual World: [%i], Interior: [%i], Adminrank: [%i], Moderator Rank: [%i]", level, PlayerInfo[user][pScore], nextLevel, GetPlayerVirtualWorld(user), GetPlayerInterior(user), PlayerInfo[user][pAdminlevel], PlayerInfo[user][pModLevel]);
				}
			}

		SendClientMessage(playerid, COLOR_GREY, lineStr);

		/* --- Line 3 --- */

		if(PlayerInfo[user][pFaction] != 0) {
			format(lineStr, sizeof(lineStr), "Faction: [%s], Rank: [%s (%i)], Driver's license: [%s], Weapon License: [%s]", FactionInfo[PlayerInfo[user][pFaction]][fName], GetFactionRank(user), PlayerInfo[user][pRank], (PlayerInfo[user][pDriveLic] != 0) ? ("Yes") : ("No"), (PlayerInfo[user][pWeaponLic] != 0) ? ("Yes") : ("No"));
		} else {
		    format(lineStr, sizeof(lineStr), "Faction: [None], Rank: [None], Driver's license: [%s], Weapon License: [%s]", (PlayerInfo[user][pDriveLic] != 0) ? ("Yes") : ("No"), (PlayerInfo[user][pWeaponLic] != 0) ? ("Yes") : ("No"));
		}

		SendClientMessage(playerid, COLOR_GREY, lineStr);

		/* --- Line 4 --- */

		for(new h = 0; h != MAX_HOUSES; h++) {
		    if(!isnull(HouseInfo[h][hOwner]) && strcmp(HouseInfo[h][hOwner], PlayerInfo[user][pName]) == 0) {
				if(HouseInfo[h][hRentalFee] > 0) {
				    format(lineStr, sizeof(lineStr), "[House] Owner: [%s], Status: [%s], Rentable: [Yes], Rental Fee: [%i], HouseID: [%i]", GetNameWithSpace(user), (HouseInfo[h][hLocked] == 0) ? ("Unlocked") : ("Locked"), HouseInfo[h][hRentalFee], HouseInfo[h][hID]);
				} else {
				    format(lineStr, sizeof(lineStr), "[House] Owner: [%s], Status: [%s], Rentable: [No], HouseID: [%i]", GetNameWithSpace(user), (HouseInfo[h][hLocked] == 0) ? ("Unlocked") : ("Locked"), HouseInfo[h][hID]);
				}

				SendClientMessage(playerid, COLOR_GREY, lineStr);
		        break;
		    }
		}

		/* --- Line 5 --- */

		if(PlayerInfo[user][pRentHouse] != 0) {
			format(lineStr, sizeof(lineStr), "[Rented House] Owner: [%s], Status: [%s], Rentable: [Yes], Rental Fee: [%i], HouseID: [%i]", GetNameWithSpaceEx(HouseInfo[PlayerInfo[user][pRentHouse]][hOwner]), (HouseInfo[PlayerInfo[user][pRentHouse]][hLocked] == 0) ? ("Unlocked") : ("Locked"), HouseInfo[PlayerInfo[user][pRentHouse]][hRentalFee], HouseInfo[PlayerInfo[user][pRentHouse]][hID]);
			SendClientMessage(playerid, COLOR_GREY, lineStr);
		}

		/* --- Line 6 --- */

		for(new b = 0; b != MAX_BUSINESSES; b++) {
		    if(!isnull(BusinessInfo[b][bOwner]) && strcmp(BusinessInfo[b][bOwner], PlayerInfo[user][pName]) == 0) {
				format(lineStr, sizeof(lineStr), "[Business] Name: [%s], Owner: [%s], Status: [%s], Fee: [$%i], BusinessID: [%i]", BusinessInfo[b][bName], GetNameWithSpace(user), (BusinessInfo[b][bLocked] == 0) ? ("Unlocked") : ("Locked"), BusinessInfo[b][bFee], BusinessInfo[b][bID]);
				SendClientMessage(playerid, COLOR_GREY, lineStr);
				break;
			}
		}

		/* --- Line 7 --- */

		switch(PlayerInfo[user][pJob]) {
			case 0: {
				format(lineStr, sizeof(lineStr), "Job: [None], Namechanges: [%i], IP: [%s], Adminjailed CPs: [%i] Jail Time: [%i minutes], Prison Time: [%i minutes], Dead: [%s]", PlayerInfo[user][pNamechanges], PlayerInfo[user][pIP], PlayerInfo[user][pAJCPs], GetPVarInt(user, "Arrest"), PlayerInfo[user][pPrisonTime], (GetPVarInt(playerid, "Death") != 0) ? ("Yes") : ("No"));
			}
			case 1: {
				format(lineStr, sizeof(lineStr), "Job: [Garbage Collector], Namechanges: [%i], IP: [%s], Adminjailed CPs: [%i], Jail Time: [%i minutes], Prison Time: [%i minutes], Dead: [%s]", PlayerInfo[user][pNamechanges], PlayerInfo[user][pIP], PlayerInfo[user][pAJCPs], GetPVarInt(user, "Arrest"), PlayerInfo[user][pPrisonTime], (GetPVarInt(playerid, "Death") != 0) ? ("Yes") : ("No"));
			}
			case 2: {
				format(lineStr, sizeof(lineStr), "Job: [Pizza Deliverer], Namechanges: [%i], IP: [%s], Adminjailed CPs: [%i], Jail Time: [%i minutes], Prison Time: [%i minutes], Dead: [%s]", PlayerInfo[user][pNamechanges], PlayerInfo[user][pIP], PlayerInfo[user][pAJCPs], GetPVarInt(user, "Arrest"), PlayerInfo[user][pPrisonTime], (GetPVarInt(playerid, "Death") != 0) ? ("Yes") : ("No"));
			}
			case 3: {
			    format(lineStr, sizeof(lineStr), "Job: [Business Delivery], Namechanges: [%i], IP: [%s], Adminjailed CPs: [%i], Jail Time: [%i minutes], Prison Time: [%i minutes], Dead: [%s]", PlayerInfo[user][pNamechanges], PlayerInfo[user][pIP], PlayerInfo[user][pAJCPs], GetPVarInt(user, "Arrest"), PlayerInfo[user][pPrisonTime], (GetPVarInt(playerid, "Death") != 0) ? ("Yes") : ("No"));
			}
			default: {
			    format(lineStr, sizeof(lineStr), "Job: [Unknown], Namechanges: [%i], IP: [%s], Adminjailed CPs: [%i], Arrested time: [%i minutes], Prison Time: [%i minutes], Dead: [%s]", PlayerInfo[user][pNamechanges], PlayerInfo[user][pIP], PlayerInfo[user][pAJCPs], GetPVarInt(user, "Arrest"), PlayerInfo[user][pPrisonTime], (GetPVarInt(playerid, "Death") != 0) ? ("Yes") : ("No"));
			}
		}

		SendClientMessage(playerid, COLOR_GREY, lineStr);

		/* --- Line 8 --- */

		for(new i = 0; i != MAX_VEHICLES; i++) {
		    if(IsValidVehicle(i) && strcmp(VehicleInfo[i][vOwner], PlayerInfo[user][pName]) == 0 && !isnull(VehicleInfo[i][vOwner])) {
				format(lineStr, sizeof(lineStr), "Spawned Vehicle: [%s], VehicleID: [%i], Color 1: [%i], Color 2: [%i], Type: [%s]", vehicleData[GetVehicleModel(i) - 400][typeName], i, VehicleInfo[i][vColor1], VehicleInfo[i][vColor2], GetVehicleTypeName(VehicleInfo[i][vType]));
				SendClientMessage(playerid, COLOR_GREY, lineStr);
		    }
		}

		/* --- Line 9 --- */

		GetPlayerHealth(user, health);
		GetPlayerArmour(user, armour);
		GetPlayerPos(user, X, Y, Z);
		format(lineStr, sizeof(lineStr), "Pos. X: [%.2f], Pos. Y: [%.2f], Pos. Z: [%.2f], Health: [%.2f], Armour: [%.2f]", X, Y, Z, health, armour);
		SendClientMessage(playerid, COLOR_GREY, lineStr);
		
		/* --- Line 10 --- */
		
