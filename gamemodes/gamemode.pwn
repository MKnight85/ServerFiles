COMMAND:stats(playerid, params[])
{

    new drank[56];
	switch(PlayerInfo[playerid][pDonateRank])
	{
		case 0: drank = "None";
		case 1: drank = "Bronze";
		case 2: drank = "Silver";
		case 3: drank = "Gold";
	}
	new lineStr[128],
	    level = floatround(floatsqroot(PlayerInfo[playerid][pScore]), floatround_floor),
	    nextLevel = floatround(floatpower(level + 1, 2));

	/* --- Header --- */

	format(lineStr, sizeof(lineStr), "Statistics of %s:", GetNameWithSpace(playerid));
	SendClientMessage(playerid, COLOR_LIGHTGREEN, lineStr);

	/* --- Line 1 --- */

	switch(PlayerInfo[playerid][pGender]) {
	    case 1: {
			format(lineStr, sizeof(lineStr), "Money: [$%i], Bank: [$%i], Savings: [$%i], Age: [%i], Gender: [Male], Level: [%i], Level Up: [%i / %i]", PlayerInfo[playerid][pMoney], PlayerInfo[playerid][pBank], PlayerInfo[playerid][pSavings], PlayerInfo[playerid][pAge], level, PlayerInfo[playerid][pScore], nextLevel);
		}
		case 2: {
        	format(lineStr, sizeof(lineStr), "Money: [$%i], Bank: [$%i], Savings: [$%i], Age: [%i], Gender: [Female], Level: [%i], Level Up: [%i / %i]", PlayerInfo[playerid][pMoney], PlayerInfo[playerid][pBank], PlayerInfo[playerid][pSavings], PlayerInfo[playerid][pAge], level, PlayerInfo[playerid][pScore], nextLevel);
	    }
	    default: {
        	format(lineStr, sizeof(lineStr), "Money: [$%i], Bank: [$%i], Savings: [$%i], Age: [%i], Gender: [Unknown], Level: [%i], Level Up: [%i / %i]", PlayerInfo[playerid][pMoney], PlayerInfo[playerid][pBank], PlayerInfo[playerid][pSavings], PlayerInfo[playerid][pAge], level, PlayerInfo[playerid][pScore], nextLevel);
		}
	}
	SendClientMessage(playerid, COLOR_GREY, lineStr);

	/* --- Line 2 --- */
