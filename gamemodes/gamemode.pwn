COMMAND:shield(playerid,params[])
{
	new string[128];
	if(PlayerInfo[playerid][pSwatCooldown] == 0)
 	{
		if(PlayerInfo[playerid][pSWAT] != 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must in the SWAT division to use this command.");
		format(string, sizeof(string), "AdmCmd: %s has used a SWAT Riot Shield, they have 250 health & armour.", GetNameWithSpace(playerid));
		SendMessageToAdmins(string);
  		SendClientMessage(playerid, COLOR_LIGHTRED, "You have enabled the Riot Shield. Your health and armour has been set to 250.");
    	PlayerInfo[playerid][pSwatCooldown] = 30;
		SetPlayerHealth(playerid, 250.0);
		SetPlayerArmour(playerid, 250.0);
	}
	else
	{
 		format(string, sizeof(string), "You must wait %i minutes before you can use the SWAT shield.", PlayerInfo[playerid][pSwatCooldown]);
		SendClientMessage(playerid, COLOR_LIGHTRED, string);
	}
	return 1;
}
