COMMAND:mdc(playerid, params[])
{
    if(PlayerInfo[playerid][pFaction] != 1) return SendClientMessage(playerid, COLOR_GREY, "You have to be a police officer of Los Angeles in order to use the Mobile Data Computer.");
	if(GetPVarInt(playerid, "LSPDDuty") == 0) return SendClientMessage(playerid, COLOR_GREY, "You have to be on duty in order to use the Mobile Data Computer.");
    if(IsPlayerInAnyVehicle(playerid) && VehicleInfo[GetPlayerVehicleID(playerid)][vFaction] == 1)
	{
		ShowPlayerDialog(playerid, DIALOG_LSPD_MDC, DIALOG_STYLE_LIST, "Mobile Data Computer", "Scan Name\nScan Plate\nScan Address\nScan Tickets\nRequest Backup\nToggle Dashcam\nScan Serial", "Choose", "Cancel");
	}
	else
	{
	    for(new m; m != sizeof(mdcs); m++)
		{
			if(IsPlayerInRangeOfPoint(playerid, 2, mdcs[m][0], mdcs[m][1], mdcs[m][2]))
			{
                ShowPlayerDialog(playerid, DIALOG_LSPD_MDC, DIALOG_STYLE_LIST, "Mobile Data Computer", "Scan Name\nScan Plate\nScan Address\nScan Tickets\nRequest Backup\nToggle Dashcam\nScan Serial", "Choose", "Cancel");
			}
		}
		SendClientMessage(playerid, COLOR_GREY, "In order to use the Mobile Data Computer you have to be either inside a Police Car or close to a computer of the LSPD.");
	}
	return 1;
}

