COMMAND:giverights(playerid, params[])
{
	new usage[128], factionid, amount, string[128], query[150], msg[128];
	if(PlayerInfo[playerid][pAdminlevel] != 6) return SendClientMessage(playerid, COLOR_LIGHTRED, "You don't have access to this command.");
    if(sscanf(params, "s[128]I(0)I(0)", usage, factionid, amount)) SendClientMessage(playerid, COLOR_GREEN, "USAGE: /giverights [weapons/drugs/scrap] [faction-ID] [type]");
	else
	{
	    if(factionid < 6 || factionid > 10) return SendClientMessage(playerid, COLOR_LIGHTRED, "You can only give faction rights to faction-ID 6 to faction-ID 10.");
	    if(strcmp(usage, "weapons", true) == 0)
		{
		    mysql_format(sqlHandle, query, sizeof(query), "UPDATE `samp_factions` SET `weapons` = %i WHERE `id` = %i", amount, factionid);
			mysql_function_query(sqlHandle, query, false, "", "");
		    if(amount > 3) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are only able to give apply rights that range from 0-3.");
			if(PlayerInfo[playerid][pAdminlevel] > 5)
			{
	   			format(msg, sizeof(msg), "*** %s has given faction-ID: %i weapon rights. (Type: %i)", GetNameWithSpace(playerid), factionid, amount);
	            SendMessageToAdmins(msg);
   			}
   			else
   			{
   			    format(string, sizeof(string), "You have given faction-ID: %d weapon rights. (Type: %i)", factionid, amount);
   				SendClientMessage(playerid, COLOR_LIGHTRED, string);
   			}
			FactionInfo[factionid][fRights_Weapons] = amount;
		}
		if(strcmp(usage, "drugs", true) == 0)
		{
		    mysql_format(sqlHandle, query, sizeof(query), "UPDATE `samp_factions` SET `drugs` = %i WHERE `id` = %i", amount, factionid);
			mysql_function_query(sqlHandle, query, false, "", "");
		    if(amount > 2) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are only able to give apply rights that range from 0-2.");
			if(PlayerInfo[playerid][pAdminlevel] > 5)
			{
	   			format(msg, sizeof(msg), "*** %s has given faction-ID: %i drugs rights. (Type: %i)", GetNameWithSpace(playerid), factionid, amount);
	            SendMessageToAdmins(msg);
   			}
   			else
   			{
   			    format(string, sizeof(string), "You have given faction-ID: %d drugs rights. (Type: %i)", factionid, amount);
   				SendClientMessage(playerid, COLOR_LIGHTRED, string);
   			}
			FactionInfo[factionid][fRights_Narcotics] = amount;
		}
		if(strcmp(usage, "scrap", true) == 0)
		{
		    mysql_format(sqlHandle, query, sizeof(query), "UPDATE `samp_factions` SET `scrap` = %i WHERE `id` = %i", amount, factionid);
			mysql_function_query(sqlHandle, query, false, "", "");
		    if(amount > 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are only able to give apply rights that range from 0-1.");
			if(PlayerInfo[playerid][pAdminlevel] > 5)
			{
	   			format(msg, sizeof(msg), "*** %s has given faction-ID: %i scrap rights. (Type: %i)", GetNameWithSpace(playerid), factionid, amount);
	            SendMessageToAdmins(msg);
   			}
   			else
   			{
   			    format(string, sizeof(string), "You have given faction-ID: %d scrap rights. (Type: %i)", factionid, amount);
   				SendClientMessage(playerid, COLOR_LIGHTRED, string);
   			}
			FactionInfo[factionid][fRights_Scrap] = amount;
		}
	}
	return 1;
}

