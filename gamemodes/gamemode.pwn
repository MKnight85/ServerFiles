			infoMsg[70],
			check = 0;

	    if(sscanf(params, "s[16]", ip)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /checkip [ip]");
	    foreach(new i : Player) {
	        if(!isnull(PlayerInfo[i][pIP]) && strfind(PlayerInfo[i][pIP], ip) != -1) {
	            check = 1;
	            format(infoMsg, sizeof(infoMsg), "%s [ID: %i] | IP: %s", GetNameWithSpace(i), i, PlayerInfo[i][pIP]);
	            SendClientMessage(playerid, COLOR_GREY, infoMsg);
	        }
	    }

		if(check == 0) {
			SendClientMessage(playerid, COLOR_GREY, "There is noone connected with this IP.");
		}
    } else {
	    SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}

	return 1;
}

