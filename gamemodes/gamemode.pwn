			    if(PlayerInfo[i][pSWAT] == 0)
			    {
		            if(GetPVarInt(i, "LSPDDuty") != 0) format(msg, sizeof(msg), "%s %s (ID: %i) ({88aa62}On-Duty{c8c8c8}).", GetFactionRank(i), GetNameWithSpace(i), i);
