ALTCOMMAND:ta->tackle;
COMMAND:tackle(playerid, params[]) {
	new user;
	if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /tackle [playerid]");
	if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
 	if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	if(GetPlayerVehicleID(playerid) != 0 || GetPlayerVehicleID(user) != 0) return SendClientMessage(playerid, COLOR_GREY, "You cannot perform a tackle on somebody while one of you is inside a vehicle.");
	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
	if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	if(GetPVarInt(playerid, "Tackled") == 1) return SendClientMessage(playerid, COLOR_GREY, "This person has already been tackled, await him to stand up before you tackle again.");
	if(playerid == user) return SendClientMessage(playerid, COLOR_GREY, "You cannot tackle yourself.");
	new Float:X,
		Float:Y,
		Float:Z,
		num = random(3);
		
	GetPlayerPos(user, X, Y, Z);
	if(!IsPlayerInRangeOfPoint(playerid, 4, X, Y, Z)) return SendClientMessage(playerid, COLOR_GREY, "You are currently not nearby the person you would like to tackle.");
	if(num < 2) {
 		new msg[150];
		format(msg, sizeof(msg), "** %s jumps off the ground towards %s in attempt to tackle %s.", GetNameWithSpace(playerid, 1), GetNameWithSpace(user, 1), HimHer(user), HimHer(user));
		ProxDetector(12, playerid, msg, COLOR_PINK);
		FreezePlayer(playerid);
		ApplyAnimation(playerid, "FINALE", "FIN_Land_Die", 4.1, 0, 1, 1, 1, 1, 1);
		ApplyAnimation(playerid, "FINALE", "FIN_Land_Die", 4.1, 0, 1, 1, 1, 1, 1);
		ToggleTackleTimer[playerid] = SetTimerEx("ToggleTackle", 5000, false, "d", playerid);
		SetPVarInt(playerid, "Tackled", 1);
		SetPVarInt(playerid, "TackledTimer", 1);
		SendClientMessage(playerid, COLOR_LIGHTRED, "Your attempt to tackle another person has failed, you will be able to get back up in 5 seconds.");
	} else {
 		new msg[150];
		format(msg, sizeof(msg), "** %s jumps off the ground towards %s in attempt to tackle %s.", GetNameWithSpace(playerid, 1), GetNameWithSpace(user, 1), HimHer(user));
		ProxDetector(12, playerid, msg, COLOR_PINK);
		FreezePlayer(playerid);
		FreezePlayer(user);
		ApplyAnimation(playerid, "PARACHUTE", "FALL_skyDive_DIE", 4.1, 0, 1, 1, 1, 1, 1);
		ApplyAnimation(user, "FINALE", "FIN_Land_Die", 4.1, 0, 1, 1, 1, 1, 1);
		ToggleTackleTimer[playerid] = SetTimerEx("ToggleTackle", 5000, false, "d", playerid);
		ToggleTackleTimer[user] = SetTimerEx("ToggleTackle", 5000, false, "d", user);
		SetPVarInt(playerid, "Tackled", 1);
		SetPVarInt(user, "Tackled", 1);
		SetPVarInt(playerid, "TackledTimer", 1);
		SetPVarInt(user, "TackledTimer", 1);
		SendClientMessage(playerid, COLOR_LIGHTGREEN, "Your attempt to tackle another person has succeeded, you will be able to get back up in 5 seconds.");
		SendClientMessage(user, COLOR_LIGHTGREEN, "A tackle-attempt of another person on you has succeeded, you will be able to get back up in 5 seconds.");
	}
	
	return 1;
}

