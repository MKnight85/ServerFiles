COMMAND:explode(playerid, params[])
{
    new string[128], user;
    if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /explode [playerid]");
    if(PlayerInfo[playerid][pAdminlevel] > 4)
    {
        new Float:x, Float:y, Float:z;
    	GetPlayerPos(user, x, y, z);
        format(string, sizeof(string), "AdmCmd: %s has exploded %s.", GetNameWithSpace(playerid), GetNameWithSpace(user));
		SendMessageToAdmins(string);
    	CreateExplosion(x, y, z, 12, 10.0);
    }
    else
    {
        SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
    }
    return 1;
}
    
COMMAND:flashbang(playerid, params[])
{
	new weaponId;
	new ammoAmount;
 	GetPlayerWeaponData(playerid, 8, weaponId, ammoAmount);
	if(PlayerInfo[playerid][pAdminlevel] > 4)
	{
	 	if (weaponId == WEAPON_GRENADE && ammoAmount > 0)
	 	{
	 	    return SendClientMessage(playerid, COLOR_LIGHTRED, "You already have a flashbang equipped. Use it first.");
	 	}

		SendClientMessage(playerid, COLOR_LIGHTRED, "You have now equipped a flashbang. You can use it to temporarily blind.");
		GivePlayerWeapon(playerid, WEAPON_GRENADE, 1);
	}
	return 1;
}

