						    format(me, sizeof(me), "** %s withdraws %s %s to send a SMS.", GetNameWithSpace(playerid, 1), HisHer(playerid), itemLowerCase(PlayerInfo[playerid][pItemType][10]));
						}

						ProxDetector(12, playerid, me, COLOR_PINK);
						PlayerPlaySound(playerid, 1053, 0, 0, 0);
						if(len + MAX_PLAYER_NAME + 7 + 7 + 14 > 128) {
						    new buffer[128];
