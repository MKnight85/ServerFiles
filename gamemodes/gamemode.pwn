COMMAND:checkdrugs(playerid, params[])
{
    new user;
    if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /checkdrugs [playerid]");
	if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
	if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
 	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
 	if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	new lineStr[128];
	new Float:X,
		Float:Y,
		Float:Z;

	GetPlayerPos(user, X, Y, Z);

	if(IsPlayerInRangeOfPoint(playerid, 2.5, X, Y, Z))
	{
		format(lineStr, sizeof(lineStr), "Drugs of %s:", GetNameWithSpace(user));
		SendClientMessage(playerid, COLOR_LIGHTGREEN, lineStr);
		format(lineStr, sizeof(lineStr), "Weed: [%d], Cocaine: [%d], Ecstasy: [%d], Crack: [%d], Heroin: [%d]", PlayerInfo[user][pWeed], PlayerInfo[user][pCocaine], PlayerInfo[user][pEcstasy], PlayerInfo[user][pCrack], PlayerInfo[user][pHeroin]);
		SendClientMessage(playerid, COLOR_GREY, lineStr);
		format(lineStr, sizeof(lineStr), "%s has searched you for drugs.", GetNameWithSpace(playerid, 1));
		SendClientMessage(user, COLOR_LIGHTRED, lineStr);
		format(lineStr, sizeof(lineStr), "You have searched %s for drugs.", GetNameWithSpace(user, 1));
		SendClientMessage(playerid, COLOR_LIGHTGREEN, lineStr);
	}
	return 1;
}
