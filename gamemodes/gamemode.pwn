COMMAND:takecall(playerid, params[]) {
	new user;
	if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /takecall [playerid]");
	if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
	if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
	if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "You have to be in your vehicle designed for this employment to accept the call.");
 	new vehicle = GetPlayerVehicleID(playerid),
 	    model = GetVehicleModel(vehicle);
 	    
 	if(model == 420 || model == 438) {
 	    if(GetPVarInt(user, "TaxiCall") == 0) return SendClientMessage(playerid, COLOR_GREY, "This player did not request a transportation lately.");
 	    if(vehicle != GetPVarInt(playerid, "PrivateCar")) return SendClientMessage(playerid, COLOR_GREY, "You have to be in your own Taxi or Cabbie in order to respond to the request.");
        new msg[105];
		SetPVarInt(user, "TaxiCall", 0);
       	SendClientMessage(user, COLOR_WHITE, "Your request is now being taken care of, await a taxi driver at your location.");
       	format(msg, sizeof(msg), "You have accepted %s's request, the customer has been informed about your coming.", GetNameWithSpace(user));
       	SendClientMessage(playerid, COLOR_WHITE, msg);
 	} else if(model == 525) {
 		if(GetPVarInt(user, "MechanicCall") == 0) return SendClientMessage(playerid, COLOR_GREY, "This player did not request a mechanic lately.");
        if(vehicle != GetPVarInt(playerid, "PrivateCar")) return SendClientMessage(playerid, COLOR_GREY, "You have to be in your own TowTruck in order to respond to an emergency.");
		new msg[105];
		SetPVarInt(user, "MechanicCall", 0);
       	SendClientMessage(user, COLOR_WHITE, "Your emergency is now being taken care of, await a mechanic at your location.");
       	format(msg, sizeof(msg), "You have accepted %s's request, the customer has been informed about your coming.", GetNameWithSpace(user));
       	SendClientMessage(playerid, COLOR_WHITE, msg);
 	} else {
 	    SendClientMessage(playerid, COLOR_GREY, "You have to be in your vehicle designed for this employment to accept the call.");
 	}
	
	return 1;
}

