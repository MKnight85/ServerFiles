COMMAND:takedrugs(playerid, params[])
{
    new user;
    if(PlayerInfo[playerid][pFaction] == 1)
	{
	    if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /takedrugs [playerid]");
