COMMAND:ame(playerid, params[]) {
	new message[128];
	if(sscanf(params, "s[128]", message)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /ame [action]");
	if(GetPVarInt(playerid, "Muted") == 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "SERVER: You are currently muted.");
	new buffer[128];
	format(buffer, sizeof(buffer), "* > %s %s", GetNameWithSpace(playerid, 1), message);
	SendClientMessage(playerid, COLOR_PINK, buffer);
	format(buffer, sizeof(buffer), "* %s %s", GetNameWithSpace(playerid, 1), message);
	SetPlayerChatBubble(playerid, buffer, COLOR_PINK, 12.0, 10000);
	return 1;
}

COMMAND:searchlocker(playerid, params[]) {
	if(PlayerInfo[playerid][pFaction] == 1 && PlayerInfo[playerid][pRank] >= FactionInfo[1][fLeaderrank] - 1) {
		new user[MAX_PLAYER_NAME];
		if(sscanf(params, "s[24]", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /searchlocker [Firstname_Lastname]");
		if(!IsValidPlayerName(user)) return SendClientMessage(playerid, COLOR_GREY, "You have to enter a username according to the Firstname_Lastname format.");
		new query[95];
		SetPVarString(playerid, "SearchLockerUsername", user);
		mysql_format(sqlHandle, query, sizeof(query), "SELECT `faction`, `badge` FROM `samp_users` WHERE `user` = '%e'", user);
		mysql_function_query(sqlHandle, query, true, "OnPlayerSearchLocker", "is", playerid, user);
	} else {
	    SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}

	return 1;
}

ALTCOMMAND:o->ooc;
COMMAND:ooc(playerid, params[]) {
	if(ooc == 1 || PlayerInfo[playerid][pAdminlevel] > 0) {
		new message[128];
	    if(sscanf(params, "s[128]", message)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /ooc [message]");
		new len = strlen(message);
		if(len + MAX_PLAYER_NAME + 14 > 128) {
		    new buffer[128];
		    strmid(buffer, message, 0, 128 - MAX_PLAYER_NAME - 14 - 4);
		    format(buffer, sizeof(buffer), "[Global OOC] %s: %s ...", GetNameWithSpace(playerid), buffer);
		    strdel(message, 0, 128 - MAX_PLAYER_NAME - 14 - 4);
			format(message, sizeof(message), "[Global OOC] %s: ... %s", GetNameWithSpace(playerid), message);
   			foreach(new i : Player) {
				SendClientMessage(i, COLOR_OOC, buffer);
				SendClientMessage(i, COLOR_OOC, message);
			}
		} else {
            format(message, sizeof(message), "[Global OOC] %s: %s", GetNameWithSpace(playerid), message);
      		SendClientMessageToAll(COLOR_OOC, message);
		}
	}  else {
	    SendClientMessage(playerid, COLOR_LIGHTRED, "SERVER: The OOC chat is currently disabled.");
	}

	return 1;
}

