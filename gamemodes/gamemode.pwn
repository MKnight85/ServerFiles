{
	new
	fields,
 		rows,
			owner[128];


	cache_get_data(rows, fields);

	if(rows == 0)
	{
		SendClientMessage(playerid, COLOR_GREY, "The cellphone you have entered does not exist.");
	}
	else
	{
		for(new i = 0; i != rows; i++)
		{
			cache_get_row(i, 0, owner);
