public OnCommandApproveAccount(playerid, username[], jailReason[], cpNumber) {
	new rows,
		fields;

    cache_get_data(rows, fields);
    if(rows == 0)
	{
        SendClientMessage(playerid, COLOR_GREY, "The account you are trying to activate has not yet been registered.");
    }
	else
	{
        if(cache_get_row_int(0, 0) > 0)
		{
			SendClientMessage(playerid, COLOR_GREY, "The account you are trying to accept has already been activated");
		}
		else
		{
            new info[110],
				query[300],
				ip[16];

            cache_get_row(0, 1, ip);
            format(info, sizeof(info), "*** %s has approved %s's account (IP: %s).", GetNameWithSpace(playerid), GetNameWithSpaceEx(username), ip);
  			foreach(new i : Player)
			{
				if(PlayerInfo[i][pAdminlevel] > 0 || PlayerInfo[i][pModLevel] > 0)
