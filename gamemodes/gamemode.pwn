			}
			else if(PlayerInfo[i][pFaction] == 2)
			{
			    if(GetPVarInt(i, "LSFDDuty") != 0) format(msg, sizeof(msg), "%s %s (ID: %i) ({88aa62}On-Duty{c8c8c8}).", GetFactionRank(i), GetNameWithSpace(i), i);
	            else format(msg, sizeof(msg), "%s %s (ID: %i).", GetFactionRank(i), GetNameWithSpace(i), i);
