/* --- Admin Commands --- */

COMMAND:kickall(playerid, params[])
{
	if(PlayerInfo[playerid][pAdminlevel] > 5)
	{
	    if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
	    new string[128], user;
	    foreach(new i : Player)
		{
		    if(PlayerInfo[i][pAdminlevel] != 5)
		    {
	    		PlayAudioStreamForPlayer(i, "http://www.losangelesrp.org/sound/itisyourtime.mp3");
	    		KickDelay(i);
    		}
 		}
	    format(string, sizeof(string), "%s %s has admin-kicked ever player from the server. (The server will restart soon)", GetAdminRank(playerid), GetNameWithSpace(playerid), GetNameWithSpace(user));
	    SendClientMessageToAll(COLOR_LIGHTRED, string);
 	}
 	return 1;
}

COMMAND:ck(playerid, params[]) {
	if(PlayerInfo[playerid][pAdminlevel] > 1) {
	    if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
	    new user;
	    if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /ck [playerid]");
	    if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
	 	if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	 	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
	    if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	    SetPVarInt(playerid, "CharacterKill", user);
		ShowPlayerDialog(playerid, DIALOG_CK, DIALOG_STYLE_MSGBOX, "{FF9900}Character Kill", "{CC3300}WARNING:{A9C4E4}\tThis operation cannot be undone. By killing this character\n\t\tthe player's statistics will be changed.\n\n{CC3300}ATTENTION:{A9C4E4}\tTake a screenshot of the player's statistics (/check [playerid])!\n\n{CC3300}QUESTION:{A9C4E4}\tWould you like to carry out this operation?", "Yes", "Cancel");
	} else {
        SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}
	
	return 1;
}

COMMAND:setnamechanges(playerid, params[])
{
	if(PlayerInfo[playerid][pAdminlevel] > 5)
	{
		if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
		new name[MAX_PLAYER_NAME],
			amount;

		if(sscanf(params, "s[24]i", name, amount)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /setnamechange [Firstname_Lastname] [amount]");
		if(!IsValidPlayerName(name)) return SendClientMessage(playerid, COLOR_GREY, "You have to enter a username according to the Firstname_Lastname format.");
		new query[75];
		mysql_format(sqlHandle, query, sizeof(query), "SELECT NULL FROM `samp_users` WHERE `user` = '%e'", name);
		mysql_function_query(sqlHandle, query, true, "OnCommandSetNamechanges", "isi", playerid, name, amount);
	}
	else
	{
	    SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}
	
	return 1;
}

COMMAND:setage(playerid, params[]) {
	if(PlayerInfo[playerid][pAdminlevel] > 0 || PlayerInfo[playerid][pModLevel] > 0) {
	    if(GetPVarInt(playerid, "StaffLogin") == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must be logged into the staff panel to access administration commands. (/stafflogin)");
		new user,
			age;
			
		if(sscanf(params, "ui", user, age)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /setage [playerid] [age]");
		if(user == INVALID_PLAYER_ID) sscanf(params, "ii", user, age);
	 	if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	 	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
	    if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	    if(age < 1 || age > 99) return SendClientMessage(playerid, COLOR_GREY, "You have to choose an age between one and ninty-nine.");
		new msg[105];
		PlayerInfo[user][pAge] = age;
