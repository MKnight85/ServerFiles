stock DistancePlayerToCP(playerid, Float:cx, Float:cy) {
	new Float:y, Float:x, Float:rst, Float:px, Float:py, Float:pz, distance;
	GetPlayerPos(playerid, px, py, pz);
	cx = floatabs(cx);
	cy = floatabs(cy);
	px = floatabs(px);
	py = floatabs(py);
	x = cx - px;
	y = cy - py;
	x = floatabs(x);
	y = floatabs(y);
	rst = floatsqroot((x*x) + (y*y));
	distance = floatround(rst);
	return distance;
}

