COMMAND:windows(playerid, params[]) {
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "You are not able to open the windows if you are not inside a vehicle.");
    new vehicle = GetPlayerVehicleID(playerid);
	if(!DoesVehicleObtainWindows(vehicle)) return SendClientMessage(playerid, COLOR_GREY, "This vehicle does not have any windows.");
	new winMsg[65];
	switch(VehicleInfo[vehicle][vWindows]) {
	    case 0: {
	        VehicleInfo[vehicle][vWindows] = 1;
	        format(winMsg, sizeof(winMsg), "** %s opens the %s's windows.", GetNameWithSpace(playerid, 1), vehicleData[GetVehicleModel(vehicle) - 400][typeName]);
	    }
	    case 1: {
	        VehicleInfo[vehicle][vWindows] = 0;
	        format(winMsg, sizeof(winMsg), "** %s closes the %s's windows.", GetNameWithSpace(playerid, 1), vehicleData[GetVehicleModel(vehicle) - 400][typeName]);
	        if(PlayerInfo[playerid][pBoombox] != -1) {
			    PlayerInfo[playerid][pBoombox] = -1;
				StopAudioStreamForPlayer(playerid);
			}
	    }
	}

	ProxDetector(12, playerid, winMsg, COLOR_PINK);
	return 1;
}

COMMAND:report(playerid, params[]) {
	new user,
		reason[128];

	if(sscanf(params, "us[128]", user, reason)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /report [playerid] [reason]");
	if(user == INVALID_PLAYER_ID) sscanf(params, "is[128]", user, reason);
	if(GetPVarInt(playerid, "ReportTimer") == 1) return SendClientMessage(playerid, COLOR_GREY, "You already reported somebody a moment ago, be patient.");
	if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
	if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	if(user == playerid) return SendClientMessage(playerid, COLOR_GREY, "You cannot report yourself.");
	new reportMsg[110],
		reasonMsg[128],
		infoMsg[100],
		playerMsg[95];
	format(reportMsg, sizeof(reportMsg), "[REPORT] {ffffff}%s (ID: %i){FF6347} has reported {ffffff}%s (ID: %i){FF6347}.", GetNameWithSpace(playerid), playerid, GetNameWithSpace(user), user);
	format(reasonMsg, sizeof(reasonMsg), "REASON: {ffffff}%s{FF6347}.", reason);
	if(PlayerInfo[user][pMasked] != 0) format(playerMsg, sizeof(playerMsg), "REPORT: You reported %s (MaskID: %i) to all connected administrators.", GetNameWithSpace(user, 1), PlayerInfo[user][pMaskID]);
	else format(playerMsg, sizeof(playerMsg), "REPORT: You reported %s (ID: %i) to all connected administrators.", GetNameWithSpace(user), user);
	format(infoMsg, sizeof(infoMsg), "[ADVICE] You may use {ffffff}/acceptreport %i {FF6347}or{ffffff} /trashreport %i{FF6347}.", playerid, playerid);
	SendClientMessage(playerid, COLOR_OOC, playerMsg);
	SetPVarInt(playerid, "Report", 1);
	SetPVarInt(playerid, "ReportTimer", 1);
	foreach(new i : Player)
	{
	    if(PlayerInfo[i][pAdminlevel] > 0)
		 {
	        SendClientMessage(i, COLOR_LIGHTRED, reportMsg);
	        SendClientMessage(i, COLOR_LIGHTRED, reasonMsg);
	        SendClientMessage(i, COLOR_LIGHTRED, infoMsg);
	    }
	}

	ReportTimerVar[playerid] = SetTimerEx("ReportTimer", 20000, false, "d", playerid);
	return 1;
}

COMMAND:helpme(playerid, params[]) {
	new help[128];
	if(sscanf(params, "s[128]", help)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /helpme [question]");
	if(GetPVarInt(playerid, "HelpmeTimer") == 1) return SendClientMessage(playerid, COLOR_GREY, "You already requested assistance a moment ago, be patient.");
	new helpMsg[128],
		question[128],
		infoMsg[59],
		count = 0;

	format(helpMsg, sizeof(helpMsg), "*** Helpme: User %s (ID: %i).", GetNameWithSpace(playerid), playerid);
	format(question, sizeof(question), "*** Question: %s", help);
	format(infoMsg, sizeof(infoMsg), "*** You may use /accepthelpme %i or /trashhelpme %i.", playerid, playerid);
	SendClientMessage(playerid, COLOR_OOC, "HELPME: You have sent a help request to all connected moderators.");
	SetPVarInt(playerid, "Helpme", 1);
	SetPVarInt(playerid, "HelpmeTimer", 1);
	foreach(new i : Player) {
	    if(PlayerInfo[i][pModLevel] > 0) {
			count++;
	    }
	}
	
	if(count > 2) {
	 	foreach(new i : Player) {
		    if(PlayerInfo[i][pModLevel] > 0) {
		        SendClientMessage(i, COLOR_OOC, helpMsg);
		        SendClientMessage(i, COLOR_OOC, question);
		        SendClientMessage(i, COLOR_WHITE, infoMsg);
		    }
		}
	} else {
	    foreach(new i : Player) {
		    if(PlayerInfo[i][pModLevel] > 0 || PlayerInfo[i][pAdminlevel] > 0) {
		        SendClientMessage(i, COLOR_OOC, helpMsg);
		        SendClientMessage(i, COLOR_OOC, question);
		        SendClientMessage(i, COLOR_WHITE, infoMsg);
		    }
		}
	}

	HelpmeTimerVar[playerid] = SetTimerEx("HelpmeTimer", 20000, false, "d", playerid);
	return 1;
}

ALTCOMMAND:l->local;
COMMAND:local(playerid, params[]) {
	new message[128];
	if(sscanf(params, "s[128]", message)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /local [message]");
	new len = strlen(message);
	if(len + MAX_PLAYER_NAME + 7 > 128) {
	    new buffer[128];
	    strmid(buffer, message, 0, 128 - MAX_PLAYER_NAME - 7 - 4);
	    format(buffer, sizeof(buffer), "%s says: %s ...", GetNameWithSpace(playerid, 1), buffer);
	    strdel(message, 0, 128 - MAX_PLAYER_NAME - 7 - 4);
		format(message, sizeof(message), "%s says: ... %s", GetNameWithSpace(playerid, 1), message);
   		ProxDetectorSplittedEx(20, playerid, buffer, message, COLOR_FADE1, COLOR_FADE2, COLOR_FADE3, COLOR_FADE4, COLOR_FADE5, 1, 0, 1);
	} else {
        format(message, sizeof(message), "%s says: %s", GetNameWithSpace(playerid, 1), message);
		ProxDetectorEx(20, playerid, message, COLOR_FADE1, COLOR_FADE2, COLOR_FADE3, COLOR_FADE4, COLOR_FADE5, 1, 0, 1);
	}

	return 1;
}

COMMAND:checkseatbelt(playerid, params[]) {
	new user;
	if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /checkseatbelt [playerid]");
	if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
	if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
 	if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
 	if(!IsPlayerInAnyVehicle(user) || !DoesVehicleObtainSeatbelts(GetPlayerVehicleID(playerid))) return SendClientMessage(playerid, COLOR_GREY, "The person is either not inside a vehicle or the vehicle does not have seatbelts.");
	new Float:X,
		Float:Y,
		Float:Z;

	GetPlayerPos(user, X, Y, Z);
	if(!IsPlayerInRangeOfPoint(playerid, 6, X, Y, Z)) return SendClientMessage(playerid, COLOR_GREY, "You have to be around the person in order to check the seatbelt.");
 	new doMsg[70],
	 	meMsg[100];

 	format(meMsg, sizeof(meMsg), "** %s checks if %s is wearing a seatbelt.", GetNameWithSpace(playerid, 1), GetNameWithSpace(user, 1));
 	if(seatbelt[user] == 1) {
	 	format(doMsg, sizeof(doMsg), "** Is wearing a seatbelt. (( %s ))", GetNameWithSpace(playerid, 1));
	} else {
	 	format(doMsg, sizeof(doMsg), "** Is not wearing a seatbelt. (( %s ))", GetNameWithSpace(playerid, 1));
	}

 	ProxDetector(12, playerid, meMsg, COLOR_PINK);
 	ProxDetector(12, playerid, doMsg, COLOR_PINK);
 	return 1;
}

COMMAND:pay(playerid, params[]) {
	new user,
		money;

	if(sscanf(params, "ui", user, money)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /pay [playerid] [amount]");
	if(user == INVALID_PLAYER_ID) sscanf(params, "ii", user, money);
	if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	if(user == playerid) return SendClientMessage(playerid, COLOR_GREY, "You cannot give yourself money.");
	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
 	if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
 	if(PlayerInfo[playerid][pScore] < 8) return SendClientMessage(playerid, COLOR_GREY, "You must have over 8 playing hours to use this command.");
	if(strcmp(PlayerInfo[playerid][pIP], PlayerInfo[user][pIP], true) == 0) return SendClientMessage(playerid, COLOR_GREY, "Money transfered blocked (Reason: Same IP).");
	new msg[128];
	if(money < GM_PAY_MIN || money > GM_PAY_MAX) {
	    format(msg, sizeof(msg), "The amount of money has to be at least $%i and has to be below $%i.", GM_PAY_MIN, GM_PAY_MAX);
		return SendClientMessage(playerid, COLOR_GREY, msg);
	}

	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Player not connected");
	if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	if(PlayerInfo[playerid][pMoney] - money < 0) return SendClientMessage(playerid, COLOR_GREY, "You do not have this amount of money.");
	new Float:X,
		Float:Y,
		Float:Z;

	GetPlayerPos(user, X, Y, Z);
	if(!IsPlayerInRangeOfPoint(playerid, 3, X, Y, Z)) return SendClientMessage(playerid, COLOR_GREY, "You are not around this player.");
	/*if(strcmp(PlayerInfo[playerid][pIP], PlayerInfo[user][pIP], true, 8) == 0) {
	    format(msg, sizeof(msg), "*** %s (ID: %i) gave %s (ID: %i) $%i whilst having a comparable IP (Use /check).", GetNameWithSpace(playerid), playerid, GetNameWithSpace(user), user, money);
	    SendMessageToAdmins(msg);
	} else */
	if(money >= GM_PAY_MAX - 5000) {
	    format(msg, sizeof(msg), "*** %s (ID: %i) gave %s (ID: %i) $%i.", GetNameWithSpace(playerid), playerid, GetNameWithSpace(user), user, money);
	    SendMessageToAdmins(msg);
	}

	GivePlayerMoneyEx(user, money);
