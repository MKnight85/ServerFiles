				{
				    if(GetPVarInt(i, "LSPDDuty") != 0) format(msg, sizeof(msg), "%s %s (ID: %i) ({88aa62}On-Duty{c8c8c8}) (SWAT).", GetFactionRank(i), GetNameWithSpace(i), i);
		            else format(msg, sizeof(msg), "%s %s (ID: %i) (SWAT).", GetFactionRank(i), GetNameWithSpace(i), i);
