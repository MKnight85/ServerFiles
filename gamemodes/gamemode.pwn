forward OnLockerCheckVolume(playerid, id, type, var);
public OnLockerCheckVolume(playerid, id, type, var) {
	new rows,
	    fields,
	    Float:vol,
	    query[128];

	cache_get_data(rows, fields);
	for(new it = 0; it != rows; it++) {
	    vol += items[cache_get_row_int(it, 0)][volume];
	}

	if(vol + items[type][volume] > GM_LOCKER_VOLUME) {
	    format(query, sizeof(query), "The lockers's capacity is not big enough (Current volume: %.2f liters, Maximum volume: %i liters, Item volume: %.2f liters).", vol, GM_LOCKER_VOLUME, items[type][volume]);
