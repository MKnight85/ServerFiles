COMMAND:stopanim(playerid, params[]) {
    if(GetPVarInt(playerid, "Frozen") != 1) {
		ApplyAnimation(playerid, "CARRY", "crry_prtial", 2.0, 0, 0, 0, 0, 0);
		switch(GetPlayerSpecialAction(playerid)) {
			case SPECIAL_ACTION_USECELLPHONE: {
			    if(GetPVarInt(playerid, "Calling") == -1) {
			        SetPlayerSpecialAction(playerid, SPECIAL_ACTION_STOPUSECELLPHONE);
					SetPVarInt(playerid, "UseCellphone", 0);
			    }
			}
			case SPECIAL_ACTION_CARRY, SPECIAL_ACTION_PISSING: {
			    if(GetPVarInt(playerid, "Handcuffed") == 0) {
				    if(GetPVarInt(playerid, "Calling") == -1) {
						SetPlayerSpecialAction(playerid, SPECIAL_ACTION_NONE);
					} else {
						SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
					}
				} else {
				    SetPlayerSpecialAction(playerid, SPECIAL_ACTION_CUFFED);
				}
			}
		}
 	} else {
	 	SendClientMessage(playerid, COLOR_GREY, "You are not able to use this command whilst being frozen/injured.");
	}

	return 1;
}

COMMAND:blockpm(playerid, params[])
{
    if(PlayerInfo[playerid][pDonateRank] == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You don't have access to this command.");
    switch(GetPVarInt(playerid, "TogglePM"))
	{
	    case 0:
		{
	        SetPVarInt(playerid, "TogglePM", 1);
	        SendClientMessage(playerid, COLOR_GREY, "Your Private Messaging System is now blocked, use /blockpm again to enable them.");
	    }
	    case 1:
		{
	        SetPVarInt(playerid, "TogglePM", 0);
	        SendClientMessage(playerid, COLOR_GREY, "Your Private Messaging System is now unblocked, you will now receive private messages again.");
