				strcat(gVar3000, "/quitjob\t\t\t{A9C4E4}Quits your current employment.");
		    }
		}
		case 10: { // Help-Center: Faction
		    if(PlayerInfo[playerid][pFaction] != 0) {
				strcat(gVar3000, "{F5F582}Command\t\tEffect\n\n{87FF87}/quitfaction\t\t{A9C4E4}Quitting current faction.\n{87FF87}/factiontalk\t\t{A9C4E4}Ability to communicate with other members of your faction.\n{87FF87}");
				strcat(gVar3000, "/members\t\t{A9C4E4}A list containing all current online members of your own faction.\n{87FF87}/togf\t\t\t{A9C4E4}Toggles the faction chat.");
			}
			if(PlayerInfo[playerid][pFaction] == 1) { // Faction: LSPD
			    strcat(gVar3000, "\n{87FF87}/gate\t\t\t{A9C4E4}Opens the gates leading to the LSPD garage.\n{87FF87}/door\t\t\t{A9C4E4}Opens doors within the LSPD's headquarter.\n{87FF87}/r\t\t\t\t{A9C4E4}Radio for intern situations.\n{87FF87}/d\t\t\t\t{A9C4E4}Radio for department overall situations.");
			    strcat(gVar3000, "\n{87FF87}/showbadge\t\t{A9C4E4}Command to withdraw and show your badge to other players.\n{87FF87}/handcuff\t\t{A9C4E4}Handcuffs another player.\n{87FF87}");
			    strcat(gVar3000, "/uncuff\t\t\t{A9C4E4}Releases a player from his handcuffs.\n{87FF87}/siren\t\t\t{A9C4E4}Adds a siren to your faction vehicle.\n{87FF87}/barriers\t\t{A9C4E4}");
			    strcat(gVar3000, "Locks/Unlocks barriers at the borders of Los Angeles.\n{87FF87}/megaphone\t\t{A9C4E4}");
			    strcat(gVar3000, "Command to speak with your megaphone.\n{87FF87}/duty\t\t\t{A9C4E4}Command to go on duty as a LSPD officer.\n{87FF87}/cite\t\t\t{A9C4E4}Issues a citation to another player.");
			    strcat(gVar3000, "\n{87FF87}/mdc\t\t\t{A9C4E4}Command to access the Mobile Data Computer.\n{87FF87}/arrest\t\t\t{A9C4E4}Arrests a player at the LSPD cells.\n{87FF87}/charge\t\t\t{A9C4E4}");
			    strcat(gVar3000, "Sets up a new charge on another player.\n{87FF87}/givelicense\t\t{A9C4E4}Command to hand out weapon licenses.");
			    strcat(gVar3000, "\n{87FF87}/addbarrier\t\t{A9C4E4}Possibility to place different types of roadblocks.\n{87FF87}/delbarrier\t\t{A9C4E4}Deletes the closest roadblock to your character.\n{87FF87}/clearbarrier");
				strcat(gVar3000, "\t\t{A9C4E4}Deletes all placed roadblocks currently on the map.\n{87FF87}/addspike\t\t{A9C4E4}Possibility to place different types of spikes.\n{87FF87}/delspike\t\t{A9C4E4}");
				strcat(gVar3000, "Deletes the closest spike to your character.\n{87FF87}/clearspike\t\t{A9C4E4}Deletes all placed spikes currently on the map.");
				strcat(gVar3000, "\n{87FF87}/createbadge\t\t{A9C4E4}Creates a new badge for a member of the faction.\n{87FF87}/deletebadge\t\t{A9C4E4}Removes the current badge of a faction member.\n{87FF87}/detain\t\t\t{A9C4E4}Detains a player in a backseat of a vehicle.");
				strcat(gVar3000, "\n{87FF87}/undetain\t\t\t{A9C4E4}Releases a player from being detained in a vehicle's backseat.\n{87FF87}/searchlocker\t\t{A9C4E4}Command to search a police officer's locker in the locker room.");
			} else if(PlayerInfo[playerid][pFaction] == 2) { // Faction: LSFD
