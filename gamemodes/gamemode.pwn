			strcat(gVar3000, "\n{87FF87}/setage\t\t\t{A9C4E4}Set's a player's age.\n{87FF87}/checktrunk\t\t{A9C4E4}Gives the ability to remove/take items from trunks.\n\n{F5F582}Note: {A9C4E4}These are administrative commands available for Administrators.");
		}
		case 2: { // Administrator-Help #2
			strcat(gVar3000, "{F5F582}Command\t\tEffect\n\n{87FF87}/mute\t\t\t{A9C4E4}Stops a player from being able to talk.\n{87FF87}/unmute\t\t{A9C4E4}Unmutes a player.\n{87FF87}/cancelinjury\t\t{A9C4E4}");
			strcat(gVar3000, "Unsets a player's death status.\n{87FF87}/setskin\t\t\t{A9C4E4}Sets a player's skin.\n{87FF87}/announcement\t\t{A9C4E4}Ability to make global announcements comparable with /ooc.\n{87FF87}");
			strcat(gVar3000, "/adminjail\t\t{A9C4E4}Jail a player, making him pass through a certain amount of pickups.\n{87FF87}/unjail\t\t\t{A9C4E4}Releases a player from administrative jail.\n{87FF87}/countdown\t\t{A9C4E4}");
			strcat(gVar3000, "Stats a global countdown on every player's screen.\n{87FF87}/destroycar\t\t{A9C4E4}Removes a vehicle from the game.\n{87FF87}");
			strcat(gVar3000, "/destroyallcars\t\t{A9C4E4}Destroys/Respawns all vehicles currently on the map.\n{87FF87}/afkers\t\t\t{A9C4E4}A list containing all player's that are currently away from keyboard.\n{87FF87}");
			strcat(gVar3000, "/specate\t\t{A9C4E4}Specates a certain player.\n{87FF87}/unban\t\t\t{A9C4E4}Unbans an account with its IP.\n{87FF87}/unbanip\t\t{A9C4E4}Unbans an IP, accounts however stay banned.\n{87FF87}/specoff\t\t{A9C4E4}Unsets an administrator from a specating.\n{87FF87}/checkweapons\t\t{A9C4E4}A list containing all weapons of a player.\n{87FF87}");
			strcat(gVar3000, "/setint\t\t\t{A9C4E4}Sets a player's interior.\n{87FF87}/checkitems\t\t{A9C4E4}Administrative tool for items of other players.\n{87FF87}/setvw\t\t\t{A9C4E4}Sets a player's virtual world.");
			strcat(gVar3000, "\n{87FF87}/gotovehicle\t\t{A9C4E4}Teleports an administrator to a vehicle.\n{87FF87}/disarm\t\t\t{A9C4E4}Disarms a player completely.\n{87FF87}");
			strcat(gVar3000, "/staffchat\t\t{A9C4E4}Ability to communicate with the Staff Team.\n{87FF87}/weather\t\t{A9C4E4}Changes to weather globally.\n{87FF87}/gotobusiness\t\t{A9C4E4}Teleports an administrator to a mentioned business.");
			strcat(gVar3000, "\n{87FF87}/sendtols\t\t{A9C4E4}Sends a player to the LSPD.\n{87FF87}/stafflogin\t\t{A9C4E4}Inevitable login for Staff Members.\n{87FF87}/jailacc\t\t\t{A9C4E4}Jails an offline player.\n{87FF87}");
			strcat(gVar3000, "/gotopos\t\t{A9C4E4}Teleports an administrator to one place on the map.\n{87FF87}/adminhide\t\t{A9C4E4}Toggles an administrator's occurance in /admins.\n{87FF87}/checkvehicle\t\t{A9C4E4}");
			strcat(gVar3000, "Ability to receive data about one vehicle on the map.\n{87FF87}/show\t\t{A9C4E4}Draws a card and shows it to a specific player.");
			strcat(gVar3000, "\n{87FF87}/putinvehicle\t\t{A9C4E4}Puts a player into a seat in a vehicle.\n{87FF87}");
			strcat(gVar3000, "/fixvehicle\t\t{A9C4E4}Repairs a vehicle completely including body.\n{87FF87}/forumname\t\t{A9C4E4}Sets an Administrators's forum name in /admins.\n\n{F5F582}Note: {A9C4E4}These are administrative commands available for Administrators.");
		}
		case 3: { // Administrator (2)-Help
		    strcat(gVar3000, "{F5F582}Command\t\tEffect\n\n{87FF87}/spawncar\t\t{A9C4E4}Spawns an administrative vehicle (Only if needed!).\n{87FF87}");
		    strcat(gVar3000, "/banacc\t\t\t{A9C4E4}Bans an offline account.\n{87FF87}/banip\t\t\t{A9C4E4}Bans an IP.\n{87FF87}");
		    strcat(gVar3000, "/toggleooc\t\t{A9C4E4}Toggles the global OOC chat for players.\n{87FF87}/createhouse\t\t{A9C4E4}Creates a new house at your current position.\n{87FF87}/deletehouse\t\t{A9C4E4}Deletes the house of the checkpoint you're in.\n{87FF87}/createbusiness\t{A9C4E4}Creates a new business at your current position.");
			strcat(gVar3000, "\n{87FF87}/deletebusiness\t\t{A9C4E4}Deletes the business of the checkpoint you're in.\n{87FF87}/setstats\t\t{A9C4E4}Ability to change certain stats of players.");
			strcat(gVar3000, "\n\n{F5F582}Note: {A9C4E4}These are administrative commands available for Senior Administrators.");
		}
		case 4: { // Lead Administrator-Help
		    strcat(gVar3000, "{F5F582}Command\t\tEffect\n\n{87FF87}/makeadmin\t\t{A9C4E4}Promotes a player into the Administration Team.");
			strcat(gVar3000, "\n{87FF87}/promoteadmin\t\t{A9C4E4}Promotes an administrator.\n{87FF87}");
		    strcat(gVar3000, "/demoteadmin\t\t{A9C4E4}Demotes an administrator.\n{87FF87}/kickadmin\t\t{A9C4E4}Kicks an administrator from the Administration Team.");
		    strcat(gVar3000, "\n{87FF87}/serverrestart\t\t{A9C4E4}Saves every player's data and restarts the gamemode.");
			strcat(gVar3000, "\n{87FF87}/makemod\t\t{A9C4E4}Promotes a player into the Moderation Team.\n{87FF87}/resstafflogin\t\t{A9C4E4}Resets a Staff Member's staff-password.");
			strcat(gVar3000, "\n{87FF87}/changename\t\t{A9C4E4}Changes a player's name and backlists the current name.\n{87FF87}/setitem\t\t{A9C4E4}Increases the amount of a certain item by one.\n{87FF87}/backup\t\t\t{A9C4E4}");
			strcat(gVar3000, "Backs up player-related data.\n{87FF87}/updatetime\t\t{A9C4E4}Updates the world's time.\n{87FF87}/removeitemtype\t{A9C4E4}Removes all items of this type from a certain player.\n{87FF87}/removeitem");
			strcat(gVar3000, "\t\t{A9C4E4}Removes one item from a player specified by its ID.\n{87FF87}/removeblacklist\t{A9C4E4}Removes a username from the server's blacklist for usernames.\n{87FF87}/msg\t\t\t{A9C4E4}Sends a global ");
			strcat(gVar3000, "message (not in chatbox).\n{87FF87}/payday\t\t\t{A9C4E4}Calls a player's pay-day and saves the according data set.\n{87FF87}/interestrate\t\t{A9C4E4}Sets the interest rate at the Bank of Los Angeles.");
			strcat(gVar3000, "\n{87FF87}/savingsrate\t\t{A9C4E4}Sets the rate for savings.\n{87FF87}/incometaxrate\t\t{A9C4E4}Sets the income tax rate.\n{87FF87}/blacklistvehicle\t{A9C4E4}Blacklists a vehicle model so that it can no longer be spawned using /spawncar.");
			strcat(gVar3000, "\n{87FF87}/unblacklistvehicle\t{A9C4E4}Removes a vehicle model from the blacklist so that it can be spawned using /spawncar.\n{87FF87}/setproperty\t\t{A9C4E4}Opens the 'Business Manager' or 'House Manager'.");
			strcat(gVar3000, "\n{87FF87}/setnamechanges\t{A9C4E4}Hands out namechange tickets.\n{87FF87}/antibunnyhop\t\t{A9C4E4}Toggles the anti-bunny-hop system.\n{87FF87}/promotemod\t\t{A9C4E4}Promotes a moderator.\n{87FF87}/demotemod\t\t{A9C4E4}Demotes a moderator.\n{87FF87}/kickmod\t\t{A9C4E4}Command to kick people from the Moderation Team.\n{87FF87}/accountpass\t\t{A9C4E4}Changes an account's password.\n\n{F5F582}Note: {A9C4E4}These are administrative commands available for Lead Administrators.");
		}
		case 5: { // Help-Center #1
            strcat(gVar3000, "{F5F582}Command\t\tEffect\n\n{87FF87}/shout\t\t\t{A9C4E4}Ability to reach more persons with you character's voice.\n{87FF87}/me\t\t\t{A9C4E4}Ability to describe your character's movements in depth");
			strcat(gVar3000, "\n{87FF87}/do\t\t\t{A9C4E4}Ability to describe roleplay situations in depth.\n{87FF87}/seatbelt\t\t{A9C4E4}Buckles in usable seatbelts in vehicles.\n{87FF87}/helmet");
            strcat(gVar3000, "\t\t\t{A9C4E4}Makes character's withdraw their helmets.\n{87FF87}/engine\t\t\t{A9C4E4}Attempt to turn on a vehicle's engine.\n{87FF87}/windows\t\t{A9C4E4}Opens a vehicle's windows.");
			strcat(gVar3000, "\n{87FF87}/pay\t\t\t{A9C4E4}Ability to hand money to other players.\n{87FF87}/b\t\t\t{A9C4E4}General OOC chat.\n{87FF87}/pickup\t\t\t{A9C4E4}Results in picking up an incoming call.\n{87FF87}");
            strcat(gVar3000, "/hangup\t\t{A9C4E4}Hangs a current call up.\n{87FF87}/low\t\t\t{A9C4E4}Results in your character speaking in a low voice.\n{87FF87}/ame\t\t\t{A9C4E4}Ability to descibe your character's movements (Not in chat!)");
			strcat(gVar3000, "\n{87FF87}/admins\t\t\t{A9C4E4}A list containing all available administrators.\n{87FF87}/tow\t\t\t{A9C4E4}Tows another vehicle using a TowTruck.\n{87FF87}/report\t\t\t{A9C4E4}Sends a report to all connected administrators.\n{87FF87}/time\t\t\t{A9C4E4}Shows the current server time and the time until your next paycheck.\n{87FF87}");
            strcat(gVar3000, "/factiontalk\t\t{A9C4E4}Ability to communicate with other members of your faction.\n{87FF87}/pm\t\t\t{A9C4E4}");
			strcat(gVar3000, "Private messaging feature (OOC).\n{87FF87}/quitfaction\t\t{A9C4E4}Quits your current faction.\n{87FF87}/ooc\t\t\t{A9C4E4}Global chat (Mostly disabled).\n{87FF87}");
            strcat(gVar3000, "/id\t\t\t{A9C4E4}Feature to search online members with only parts of names.\n{87FF87}/namechange\t\t{A9C4E4}Changes your character's name.\n{87FF87}/animhelp\t\t{A9C4E4}List containing commands for different animations.");
			strcat(gVar3000, "\n{87FF87}/stopanim\t\t{A9C4E4}Stopping the current usage of animations.\n{87FF87}/items\t\t\t{A9C4E4}A list of items that you currently have in your pockets.\n{87FF87}/pickitem\t\t{A9C4E4}Picks an item up from the ground.");
			strcat(gVar3000, "\n{87FF87}/giveitem\t\t{A9C4E4}Passes an item over to another player.\n{87FF87}/local\t\t\t{A9C4E4}Usefull feature to talk locally while talking on the phone.");
		}
		case 6: { // Help-Center #2
			strcat(gVar3000, "{F5F582}Command\t\tEffect\n\n{87FF87}/trunk\t\t\t{A9C4E4}Administration of trunks nearby.\n{87FF87}/stats\t\t\t{A9C4E4}Displaying data about your account.\n{87FF87}/mods\t\t\t{A9C4E4}");
			strcat(gVar3000, "A list containing all available moderators.\n{87FF87}/helpme\t\t{A9C4E4}In need of help? Inform moderators!\n{87FF87}/changepass");
			strcat(gVar3000, "\t\t{A9C4E4}Changes your password.\n{87FF87}/chatstyle\t\t{A9C4E4}Changes the animation applied upon speaking.\n{87FF87}/viewcitations\t\t{A9C4E4}A list containing all unpaid citations.");
			strcat(gVar3000, "\n{87FF87}/showid\t\t\t{A9C4E4}Shows your identity card to another player.\n{87FF87}/whisper\t\t{A9C4E4}Whispering Feature to talk to other players in a very low voice.");
			strcat(gVar3000, "\n{87FF87}/c\t\t\t{A9C4E4}Usefull feature when having to talk while being in an animation.\n{87FF87}");
			strcat(gVar3000, "/checkseatbelt\t\t{A9C4E4}Checks whether another player's seatbelt is buckled in.\n{87FF87}/call\t\t\t{A9C4E4}Dials a number.\n{87FF87}/sms\t\t\t{A9C4E4}Sends a text message to a certain number.\n{87FF87}/blockpm\t\t{A9C4E4}Blocks the ability to be messages via /pm.");
			strcat(gVar3000, "\n{87FF87}/tackle\t\t\t{A9C4E4}Tackles another player.\n{87FF87}/walkstyle\t\t{A9C4E4}Changes the set animation to walk.\n{87FF87}");
			strcat(gVar3000, "/lpm\t\t\t{A9C4E4}Messages the last person sending you a message via /pm.\n{87FF87}/sellfish\t\t\t{A9C4E4}Sell your fish at the fish factory.");
			strcat(gVar3000, "\n{87FF87}/showlicenses\t\t{A9C4E4}Shows your licenses to another player.\n{87FF87}/greet\t\t\t{A9C4E4}Offers another player a handshake.\n{87FF87}/clearchat\t\t{A9C4E4}Clears your personal chat.\n{87FF87}/showcons\t\t{A9C4E4}Feature to get messaged about connecting players.");
			strcat(gVar3000, "\n{87FF87}/tognews\t\t{A9C4E4}Toggles the news published by the San Andreas Network.\n{87FF87}/togmap\t\t{A9C4E4}Toggles the area information in the left botton corner.\n{87FF87}/frisk\t\t\t{A9C4E4}Sends a frisk-request to a player.\n{87FF87}/acceptfrisk\t\t{A9C4E4}Initiates a frisk which has been requested beforehand.");
			strcat(gVar3000, "\n{87FF87}/jobhelp\t\t{A9C4E4}Extra command-help for your job.\n{87FF87}/househelp\t\t{A9C4E4}Extra command-help for your house.\n{87FF87}/businesshelp\t\t{A9C4E4}Extra command-help for your business.\n{87FF87}/togafk\t\t\t{A9C4E4}Toggles the message showing how long you have been away from keyboard.");
			strcat(gVar3000, "\n{87FF87}/factionhelp\t\t{A9C4E4}Extra command-help for your faction.\n{87FF87}/taxihelp\t\t{A9C4E4}Extra command-help for taxi drivers.\n{87FF87}/mechanichelp\t\t{A9C4E4}Extra command-help for mechanics.\n{87FF87}/vehiclehelp\t\t{A9C4E4}Extra command-help for vehicle administration.");
		}
		case 7: { // Help-Center: Business
			strcat(gVar3000, "{F5F582}Command\t\tEffect\n\n{87FF87}/buybusiness\t\t{A9C4E4}Possibility to buy a business for a certain price.\n{87FF87}/sellbusiness\t\t{A9C4E4}Sell your business and receive 75 percent of its price.\n{87FF87}/business\t\t{A9C4E4}Ability to administrate your business individually.");
		}
		case 8: { // Help-Center: House
		    strcat(gVar3000, "As a deliver, you have to arrive Ocean Docks, enter a van and turn on the engine.\n The GPS system will mark the delivery location on your minimap and you have to go there.");
			strcat(gVar3000,"\n{F5F582}Command\t\tEffect\n\n{87FF87}/buyhouse\t\t{A9C4E4}Possibility to buy a house for a certain price.\n{87FF87}/sellhouse\t\t{A9C4E4}Sell a house and receive 75 percent of its price.\n{87FF87}/house");
		    strcat(gVar3000, "\t\t\t{A9C4E4}Ability to administrate your house individually.\n{87FF87}/renthouse\t\t{A9C4E4}Possibility to rent at any house.\n{87FF87}/unrenthouse\t\t{A9C4E4}Stop renting at any house you are renting at.\n{87FF87}");
			strcat(gVar3000, "/wardrobe\t\t{A9C4E4}Access to your wardrobe at home.");
		} 
		case 9: { // Help-Center: Job
		    if(PlayerInfo[playerid][pJob] == 1)
			{ // Job: Garbage Collector
		    	strcat(gVar3000, "The Garbage Job consists of players operating a Garbage Collector and driving throughout the town to pick up it's trash from dumpsters.\n The player will visit the designated locations and fill up the truck before unloading at it's final destination.");
		        strcat(gVar3000, "\n{F5F582}Command\t\tEffect\n\n{87FF87}/taketrash\t\t{A9C4E4}Reaching for a trash bag in a garbage container.\n{87FF87}/puttrash\t\t{A9C4E4}Throwing a trash bag into a Trashmaster.\n");
				strcat(gVar3000, "{87FF87}/unload\t\t\t{A9C4E4}Unloads a Trashmaster's trash bags.\n{87FF87}/quitjob\t\t\t{A9C4E4}Quits your current employment.");
		    } else if(PlayerInfo[playerid][pJob] == 2)
			{ // Job: Pizza Deliverer
		    	strcat(gVar3000, "The Pizza Job consists of players operating a pizza's deliveries and driving throughout the town to supply the clients.\n In order to start, go to Idlewood pizza stack and fill your bike with the boxes. Thereafter, turn on the engine and follow your GPS.");
				strcat(gVar3000, "\n{F5F582}Command\t\tEffect\n\n{87FF87}/takepizza\t\t{A9C4E4}Withdraws a pizza box from your pizza bike.\n{87FF87}/putpizza\t\t{A9C4E4}Places a pizza box into a pizza bike's box.\n{87FF87}");
				strcat(gVar3000, "/quitjob\t\t\t{A9C4E4}Quits your current employment.");
		    } else if(PlayerInfo[playerid][pJob] == 3)
