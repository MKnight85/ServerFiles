	        SetPVarInt(playerid, "CurrentItemPage", 1);
			ShowPlayerDialog(playerid, DIALOG_LOCKER_STORE, DIALOG_STYLE_LIST, "{FF9900}Locker", dialogMsg, "Store", "Cancel");
		}
	}
