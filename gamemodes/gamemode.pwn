    				format(dialogMsg, sizeof(dialogMsg), "%s\n%s [%i liters]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
				} else if(pItems[i] == 17) {
				    format(dialogMsg, sizeof(dialogMsg), "%s\n%s [%i/21 bullets]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
				} else if(pItems[i] == 18) {
				    format(dialogMsg, sizeof(dialogMsg), "%s\n%s [%i/100 bullets]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
				} else if(pItems[i] == 19) {
				    format(dialogMsg, sizeof(dialogMsg), "%s\n%s [%i/20 rounds]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
                } else if(pItems[i] == 28 || (pItems[i] >= 244 && pItems[i] <= 249)) {
				    format(dialogMsg, sizeof(dialogMsg), "%s\n%s [%i ml]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
				} else {
				    format(dialogMsg, sizeof(dialogMsg), "%s\n%s", dialogMsg, items[pItems[i]][itemName]);
				}
			}

			if(getItemID(playerid, i) == PlayerInfo[playerid][pItemID][items[pItems[i]][iIdx]]) {
			    format(dialogMsg, sizeof(dialogMsg), "%s (In Use)", dialogMsg);
			}
		}

		if(itemIndexExists(playerid, 21)) {
			format(dialogMsg, sizeof(dialogMsg), "%s\n{FF9900}Next Page >>", dialogMsg);
		}

        SetPVarInt(user, "CurrentItemPage", 1);
		ShowPlayerDialog(user, DIALOG_FRISK, DIALOG_STYLE_LIST, "{FF9900}Items", dialogMsg, "Take", "Cancel");
	} else {
		SendClientMessage(user, COLOR_GREY, "This player does not possess any items.");
	}

	return 1;
}

