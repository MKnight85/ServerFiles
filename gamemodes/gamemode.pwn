COMMAND:hotwire(playerid, params[])
{
    new vehicle = GetPlayerVehicleID(playerid), Float:health, msg[128], vengine;
    new model = GetVehicleModel(vehicle);
	GetVehicleHealth(vehicle, health);
	new
		lights,
			alarm,
		 		doors,
		 			bonnet,
		 				boot,
		 					objective;

	GetVehicleParamsEx(vehicle, vengine, lights, alarm, doors, bonnet, boot, objective);
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "You have to be inside a vehicle to hotwire the engine.");
	if(GetPlayerVehicleSeat(playerid) != 0) return SendClientMessage(playerid, COLOR_GREY, "You have to be the driver of the vehicle to hotwire the engine.");
	if(health < GM_BREAKDOWN_HEALTH) return SendClientMessage(playerid, COLOR_GREY, "The vehicle's engine is broken down, you are not able to hotwire the vehicle.");
	if(VehicleInfo[vehicle][vFuel] == 0) return SendClientMessage(playerid, COLOR_GREY, "The vehicle is out of fuel, you are not able to hotwire the vehicle.");
	if(!IsAnEngineCar(vehicle)) return SendClientMessage(playerid, COLOR_GREY, "This vehicle does not have an engine.");
	if(GetPVarInt(playerid, "turningengine") != 0) return SendClientMessage(playerid, COLOR_GREY, "The engine is already being hotwire, be patient.");
	if(VehicleInfo[vehicle][vType] == VEHICLE_OWNED)
	{
		switch(vengine)
		{
			case 0:
			{
			    if(PlayerInfo[playerid][pToolKit] == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "You must have a tool kit to use this command.");
				format(msg, sizeof(msg), "** %s uses their tools to try and hotwire the %s's engine.", GetNameWithSpace(playerid, 1), vehicleData[model - 400][typeName]);
				ProxDetector(12, playerid, msg, COLOR_PINK);
				SetTimerEx("engine", 15000, false, "di", playerid, vehicle);
				SetPVarInt(playerid, "turningengine", 1);
				GameTextForPlayer(playerid, "~n~~n~~n~~n~~n~~n~~n~~g~Hotwiring", 15000, 3);
				PlayerInfo[playerid][pToolKit] --;
				return 1;
			}
			case 1:
			{
				SetVehicleParamsEx(vehicle, 0, lights, alarm, doors, bonnet, boot, objective);
				format(msg, sizeof(msg), "** %s cuts the %s's engine.", GetNameWithSpace(playerid, 1), vehicleData[model - 400][typeName]);
				ProxDetector(12, playerid, msg, COLOR_PINK);
				return 1;
			}
		}
	}
	else
	{
        SendClientMessage(playerid, COLOR_LIGHTRED, "You are only able to hotwire an ownable vehicle.");
	}
	return 1;
}

