			mysql_format(sqlHandle, query, sizeof(query), "INSERT INTO `samp_registrations` (`account`, `ip`, `approver`, `date`) VALUES ('%e', '%e', '%e', NOW())", GetNameWithSpaceEx(username), ip, GetNameWithSpace(playerid));
