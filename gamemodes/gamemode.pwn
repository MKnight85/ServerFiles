	if(PlayerInfo[playerid][pModLevel] > 0) {
	    new message[128];
	    if(sscanf(params, "s[128]", message)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /modtalk [message]");
		new len = strlen(message);
		if(len + MAX_PLAYER_NAME + 21 > 128) {
		    new buffer[128];
