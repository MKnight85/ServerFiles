COMMAND:undetain(playerid, params[]) {
    if(PlayerInfo[playerid][pFaction] == 1) {
	    if(GetPVarInt(playerid, "LSPDDuty") == 0) return SendClientMessage(playerid, COLOR_GREY, "You have to be on-duty as a police officer in order to release people from being detained.");
		new user;
		if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /undetain [playerid]");
		if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
		if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
		if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
		if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
		if(GetPVarInt(user, "detained") == 0) return SendClientMessage(playerid, COLOR_GREY, "The player you attempt to release is not detained.");
		if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "You cannot release a player from being detained whilst being inside a vehicle yourself.");
		if(user == playerid) return SendClientMessage(playerid, COLOR_GREY, "You cannot release yourself from being detained.");
		new msg[100];
		UnfreezePlayer(user);
		SendClientMessage(user, COLOR_WHITE, "You are no longer detained, you are now able to exit the vehicle.");
		format(msg, sizeof(msg), "%s is no longer detained, he is now able to exit the vehicle by himself.", GetNameWithSpace(user, 1));
		SendClientMessage(playerid, COLOR_WHITE, msg);
	} else {
	    SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}

	return 1;
}

COMMAND:detain(playerid, params[]) {
    if(PlayerInfo[playerid][pFaction] == 1) {
	    if(GetPVarInt(playerid, "LSPDDuty") == 0) return SendClientMessage(playerid, COLOR_GREY, "You have to be on-duty as a police officer in order to detain people.");
		new user;
		if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /detain [playerid]");
		if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
		if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
		if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
		if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
		if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "You cannot detain another player whilst being inside a vehicle yourself.");
		if(!IsPlayerInAnyVehicle(user)) return SendClientMessage(playerid, COLOR_GREY, "In order to detain a player, the player has to be inside a vehicle.");
		new seat = GetPlayerVehicleSeat(user);
		if(seat != 2 && seat != 3) return SendClientMessage(playerid, COLOR_GREY, "You are just able to detain people in the backseats.");
		if(user == playerid) return SendClientMessage(playerid, COLOR_GREY, "You cannot detain yourself.");
		new vehicle = GetPlayerVehicleID(user),
		    vData[7];

		GetVehicleParamsEx(vehicle, vData[0], vData[1], vData[2], vData[3], vData[4], vData[5], vData[6]);
		if(vData[3] != 0) return SendClientMessage(playerid, COLOR_GREY, "You are just able to detain a player as long as the vehicle's doors are unlocked.");
		new	Float:X,
		    Float:Y,
		    Float:Z,
			msg[128];

		GetPlayerPos(user, X, Y, Z);
		if(!IsPlayerInRangeOfPoint(playerid, 3.5, X, Y, Z)) return SendClientMessage(playerid, COLOR_GREY, "In order to detain another player, you have to be nearby that player.");
		FreezePlayer(user);
		SetPVarInt(user, "detained", vehicle);
		format(msg, sizeof(msg), "** %s detains %s in one of the %s's backseats.", GetNameWithSpace(playerid, 1), GetNameWithSpace(user, 1), vehicleData[GetVehicleModel(vehicle) - 400][typeName]);
		ProxDetector(12, playerid, msg, COLOR_PINK);
    } else {
	    SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}

	return 1;
}

COMMAND:frisk(playerid, params[]) {
