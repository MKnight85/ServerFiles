					break;
				}

				if(isnull(dialogMsg)) {
				    if(pItems[i] >= 4 && pItems[i] <= 13) {
						format(dialogMsg, sizeof(dialogMsg), "%s [Number: %i]", items[pItems[i]][itemName], getItemVar(playerid, i));
					} else if(pItems[i] == 3) {
		   				format(dialogMsg, sizeof(dialogMsg), "%s [%i liters]", items[pItems[i]][itemName], getItemVar(playerid, i));
					} else if(pItems[i] == 17) {
					    format(dialogMsg, sizeof(dialogMsg), "%s [%i/21 bullets]", items[pItems[i]][itemName], getItemVar(playerid, i));
					} else if(pItems[i] == 18) {
					    format(dialogMsg, sizeof(dialogMsg), "%s [%i/100 bullets]", items[pItems[i]][itemName], getItemVar(playerid, i));
					} else if(pItems[i] == 19) {
					    format(dialogMsg, sizeof(dialogMsg), "%s [%i/20 rounds]", items[pItems[i]][itemName], getItemVar(playerid, i));
                    } else if(pItems[i] == 28 || (pItems[i] >= 244 && pItems[i] <= 249)) {
				    	format(dialogMsg, sizeof(dialogMsg), "%s [%i ml]", items[pItems[i]][itemName], getItemVar(playerid, i));
					} else {
					    format(dialogMsg, sizeof(dialogMsg), "%s", items[pItems[i]][itemName]);
					}
				} else {
					if(pItems[i] >= 4 && pItems[i] <= 13) {
						format(dialogMsg, sizeof(dialogMsg), "%s\n%s [Number: %i]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
					} else if(pItems[i] == 3) {
		   				format(dialogMsg, sizeof(dialogMsg), "%s\n%s [%i liters]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
					} else if(pItems[i] == 17) {
					    format(dialogMsg, sizeof(dialogMsg), "%s\n%s [%i/21 bullets]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
					} else if(pItems[i] == 18) {
					    format(dialogMsg, sizeof(dialogMsg), "%s\n%s [%i/100 bullets]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
					} else if(pItems[i] == 19) {
					    format(dialogMsg, sizeof(dialogMsg), "%s\n%s [%i/20 rounds]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
                    } else if(pItems[i] == 28 || (pItems[i] >= 244 && pItems[i] <= 249)) {
				    	format(dialogMsg, sizeof(dialogMsg), "%s\n%s [%i ml]", dialogMsg, items[pItems[i]][itemName], getItemVar(playerid, i));
					} else {
					    format(dialogMsg, sizeof(dialogMsg), "%s\n%s", dialogMsg, items[pItems[i]][itemName]);
					}
				}

				if(getItemID(playerid, i) == PlayerInfo[playerid][pItemID][items[pItems[i]][iIdx]]) {
