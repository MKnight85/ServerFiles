		//AdminMessageLog(healthHack, Target, "HACK WARNING - Health");
		SetPVarInt(Target, "AntiHealthHack2", 1);
		AntiHealthHackTimerVar2[Target] = SetTimerEx("AntiHealthHackTimer2", 60000, false, "i", Target);
	}

    return 1;
}

