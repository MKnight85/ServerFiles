forward BreakInVehicle(playerid);
public BreakInVehicle(playerid)
{
    new vdata[7];
	new vehicle = GetNearestVehicle(playerid, 3.0);
	new zName[MAX_ZONE_NAME];
	GetPlayerZone(playerid, zName, sizeof(zName));
    GetVehicleParamsEx(vehicle, vdata[0], vdata[1], vdata[2], vdata[3], vdata[4], vdata[5], vdata[6]);
	if(IsPlayerInRangeOfPoint(playerid, 3.0, BreakX[playerid], BreakY[playerid], BreakZ[playerid]))
	{
		switch(random(2))
		{
			case 1:
			{
				SetVehicleParamsEx(vehicle, vdata[0], vdata[1], vdata[2], 0, vdata[4], vdata[5], vdata[6]);
				GameTextForPlayer(playerid, "~g~Unlocked", 750, 4);
				SendClientMessage(playerid, COLOR_BRIGHTGREEN, "You have suceeded on your attempt to break into this vehicle.");
				IsBreakingIn[playerid] = 0;
			    BreakX[playerid] = 0.0;
			    BreakY[playerid] = 0.0;
			    BreakZ[playerid] = 0.0;
			    BreakDownTimer[playerid] = 0;
			    IsBreaking[playerid] = 0;
			    if(VehicleInfo[vehicle][vLockType] > 2)
			    {
			    	CarTheft(playerid);
		    	}
			}
			default:
			{
			
			    SendClientMessage(playerid, COLOR_LIGHTRED, "You have failed on your attempt to break into this vehicle.");
			    IsBreakingIn[playerid] = 0;
			    BreakX[playerid] = 0.0;
			    BreakY[playerid] = 0.0;
			    BreakZ[playerid] = 0.0;
			    BreakDownTimer[playerid] = 0;
			    IsBreaking[playerid] = 0;
			}
		}
	}
	return 1;
}

