		        dmvdrive[playerid] = 14;
		    }
		    case 14: {
		        SetPlayerRaceCheckpoint(playerid, 2, 1084.0590, -1131.8002, 23.5452, 0, 0, 0, 4);
		        dmvdrive[playerid] = 15;
		    }
		    case 15: {
		        SetPlayerRaceCheckpoint(playerid, 2, 1084.7467, -1047.3859, 31.5711, 0, 0, 0, 4);
