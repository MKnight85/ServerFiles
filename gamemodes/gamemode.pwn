		    }
		}
	} else if(GetPVarInt(playerid, "Pizza") == 1 && PlayerInfo[playerid][pJob] == 2) {
	    SendClientMessage(playerid, COLOR_WHITE, "In order to finish the delivery, withdraw the pizza from the pizza bike (( /takepizza )) and walk towards the house's porch.");
	} else if(GetPVarInt(playerid, "Trashmaster") == 1 && PlayerInfo[playerid][pJob] == 1) {
        SendClientMessage(playerid, COLOR_WHITE, "In order to collect garbage, step close to the garbage container and start loading your Trashmaster (( /taketrash )).");
