COMMAND:tent(playerid, params[])
{
    new idx[10];
    if(sscanf(params, "s[10]", idx)) return SendClientMessage(playerid, COLOR_LIGHTGREEN, "/tent {ffffff}[disassemble/establish]");
 	if(!strcmp(idx, "disassemble", true, 9))
	{
		if(PlayerInfo[playerid][pTentID] == -1) return SendClientMessage(playerid, COLOR_LIGHTRED, "You don't have a tent.");
		ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.1, 1, 1, 1, 0, 100000, 1);
		SetPVarInt(playerid, "Muted", 1);
		PlayerInfo[playerid][pTentTimer] = SetTimerEx("Tent_ProcedureTimer",1000,0,"di",playerid, 1);
		PlayerInfo[playerid][pTentTimerVar] = 0;
		PlayerInfo[playerid][pTentProcess] = 1;
		return SendClientMessage(playerid, 0x0473ADFF, "[Information]{ffffff} To proceed with the procedure, please type /tent establish around it.");
	}
	if(!strcmp(idx, "establish", true, 9))
	{
		new
			TentID = -1,
			hashammer = 0;
	    for(new i = 0; i <= 10; i++)
	    {
	    	if(getPlayerItemID(playerid, 25)) hashammer++;
		}
		if(hashammer <= 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "You need a hammer to proceed with the procedure.");
		for(new i = 0; i < MAX_TENTS; i++)
		{
		    if(IsPlayerInRangeOfPoint(playerid, 3, TentInfo[i][tPos][0], TentInfo[i][tPos][1], TentInfo[i][tPos][2]))
		    {
		        TentID = i;
		        break;
		    }
		}
		if(TentID == -1) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are not close to your tent's package.");
		SetPVarInt(playerid, "Muted", 1);
        PlayerInfo[playerid][pTentTimer] = SetTimerEx("Tent_ProcedureTimer",1000,0,"di",playerid, 2);
		PlayerInfo[playerid][pTentTimerVar] = 0;
