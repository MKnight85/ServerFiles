	    new user[MAX_PLAYER_NAME], query[90];
	    if(sscanf(params, "s[24]", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /unban [Firstname_Lastname]");
	    if(!IsValidPlayerName(user)) return SendClientMessage(playerid, COLOR_GREY, "You have to enter a username according to the Firstname_Lastname format.");
	    mysql_format(sqlHandle, query, sizeof(query), "SELECT `ip` FROM `samp_users` WHERE `user` = '%e'", user);
