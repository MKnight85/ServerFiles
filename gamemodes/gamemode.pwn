	 	{
	 	    if(PlayerInfo[playerid][pAdminlevel] == 0)
	 	    {
			    SetPlayerStandardSkin(playerid);
			    SendClientMessage(playerid, COLOR_LIGHTRED, "SERVER: You are not allowed to wear Carl Johnson's outfit (CJ Skin).");
   			}
		}
	}

	return 1;
}

