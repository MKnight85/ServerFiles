COMMAND:staffchat(playerid, params[]) {
	if(PlayerInfo[playerid][pAdminlevel] > 0 || PlayerInfo[playerid][pModLevel] > 0) {
	    new message[128];
	    if(sscanf(params, "s[128]", message)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /staffchat [message]");
