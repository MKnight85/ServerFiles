COMMAND:interview(playerid, params[]) {
	if(PlayerInfo[playerid][pFaction] == 3) {
	    if(GetPVarInt(playerid, "SanDuty") > 0) {
	        if(sanInterviewRequest[2] != 0 && sanInterviewRequest[1] == playerid) {
	            SendClientMessage(playerid, COLOR_LIGHTRED, "You have stopped broadcasting the interview.");
	            SendClientMessage(sanInterviewRequest[0], COLOR_LIGHTRED, "The SANews Reporter has stopped broadcasting the interview.");
	            sanInterviewRequest[0] = -1;
	            sanInterviewRequest[1] = -1;
	            sanInterviewRequest[2] = 0;
			} else {
			    new user;
				if(sscanf(params, "i", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /interview [playerid]");
		        if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
				if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
				if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
				if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
				if(playerid == user) return SendClientMessage(playerid, COLOR_GREY, "You cannot interview yourself.");
				if(sanInterviewRequest[2] != 0) return SendClientMessage(playerid, COLOR_GREY, "There is already an interview being broadcasted.");
				new msg[115];
				format(msg, sizeof(msg), "%s has been asked to join the interview, await the player's acceptance.", GetNameWithSpace(user, 1));
				SendClientMessage(playerid, COLOR_WHITE, msg);
				format(msg, sizeof(msg), "SANews Reporter %s asks you to join an interview, use /joininterview to join the interview.", GetNameWithSpace(playerid, 1));
				SendClientMessage(user, COLOR_ADVERT, msg);
				sanInterviewRequest[0] = user;
				sanInterviewRequest[1] = playerid;
			}
	    } else {
		    SendClientMessage(playerid, COLOR_GREY, "You have to go on duty and equip your equipment before being able to interview people.");
		}
	} else {
	    SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
 	}
 	
	return 1;
}

