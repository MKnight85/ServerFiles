	{
    	if(serverrestart == 1)
		{
    	    /* --- Kicking connecting users if server is about to restart --- */

		    SendClientMessage(playerid, COLOR_LIGHTRED, "SERVER: The server is currently restarting, please reconnect in a moment.");
			KickDelay(playerid);
			return 1;
		}
		else
		{
		    GetPlayerName(playerid, PlayerInfo[playerid][pName], MAX_PLAYER_NAME);
			if(!IsValidPlayerName(PlayerInfo[playerid][pName]))
			{
			    /* --- Kicking connecting users with wrong Username-Format --- */
                if(strcmp(PlayerInfo[playerid][pName], "Hash") != 0)
   				{
					SendClientMessage(playerid, COLOR_LIGHTGREEN, "SERVER: You must have a roleplay name to play on this server. Format: Firstname_Lastname.");
					SendClientMessage(playerid, COLOR_LIGHTGREEN, "SERVER: At least three characters before and after the underscore and only characters like a-z and A-Z.");
