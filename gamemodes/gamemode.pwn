COMMAND:prison(playerid, params[])
{
	if(PlayerInfo[playerid][pFaction] != 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	new
		user,
		time,
		msg[200],
		reason[128],
		string[128];

	if(sscanf(params, "uis[128]", user, time, reason)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /prison [playerid] [minutes] [reason]");
	if(user == INVALID_PLAYER_ID) sscanf(params, "ii", user, time);
	if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	if(!IsPlayerInRangeOfPoint(playerid, 5, 1807.2378,-1538.6150,5700.4287)) return SendClientMessage(playerid, COLOR_GREY, "You are not at the San Andreas Prison Facility.");
	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
	if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	if(!IsPlayerInRangeOfPoint(user, 5, 1807.2378,-1538.6150,5700.4287)) return SendClientMessage(playerid, COLOR_GREY, "The suspect must also be near the San Andreas Prison Facility.");
	if(time < 60 || time > 600) return SendClientMessage(playerid, COLOR_GREY, "To prison at San Andreas Prison Facility you must pick between 60 to 600 minutes. (1 hour to 10 hours)");
	if(user == playerid) return SendClientMessage(playerid, COLOR_GREY, "You are unable to prison yourself.");
	SetPlayerPosEx(user, 5, 154.7240,1406.3398,10.5950, 0, 0, 0);
	SetCameraBehindPlayer(user);
	format(msg, sizeof(msg), "You have been arrested by the Los Angeles Police Department for %i minutes.", time);
	SendClientMessage(user, COLOR_WHITE, msg);
	format(string, sizeof(string), "LAPD: %s has been sent to San Andreas Prison Facility by %s for %i minutes.", PlayerInfo[user][pName], PlayerInfo[playerid][pName], time);
	SendClientMessageToAll(COLOR_LIGHTRED, string);
	PlayerInfo[user][pPrisonTime] = time;
	PlayerInfo[user][pPrisoned] = 1;
	PlayerInfo[user][pCuffed] = 0;
	SetPlayerSpecialAction(user, SPECIAL_ACTION_NONE);
	mysql_format(sqlHandle, msg, sizeof(msg), "INSERT INTO `samp_arrests` (`user`, `time`, `reason`) VALUES ('%e', %i, '%e')", PlayerInfo[user][pName], time, reason);
	mysql_function_query(sqlHandle, msg, false, "", "");
	return 1;
}

COMMAND:arrest(playerid, params[]) {
	if(PlayerInfo[playerid][pFaction] != 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	new
		user,
		time,
		cell,
		reason[128],
		string[128];
		
	if(sscanf(params, "uiis[128]", user, time, cell, reason)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /arrest [playerid] [minutes] [cell] [reason]");
	if(user == INVALID_PLAYER_ID) sscanf(params, "iii", user, time, cell);
	if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	if(!IsPlayerInRangeOfPoint(playerid, 5, 221.6334, 114.6567, 999.0156)) return SendClientMessage(playerid, COLOR_GREY, "You are not around the cells of the Los Angeles Police Department.");
	if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
	if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	if(cell < 1 || cell > 4) return SendClientMessage(playerid, COLOR_GREY, "You have to choose one of the cells in the Los Angeles Police Department (1 - 4).");
	if(!IsPlayerInRangeOfPoint(user, 5, 221.6334, 114.6567, 999.0156)) return SendClientMessage(playerid, COLOR_GREY, "The suspect you would like to arrest is not around the cells.");
	if(user == playerid) return SendClientMessage(playerid, COLOR_GREY, "You are not able to arrest yourself.");
	new msg[200];
	switch(cell) {
		case 1: {
			SetPlayerPosEx(user, 5, 215.1684, 109.9195, 999.0156, 0, GetPlayerInterior(user), GetPlayerVirtualWorld(user));
		}
		case 2: {
			SetPlayerPosEx(user, 5, 219.2462, 109.9087, 999.0156, 0, GetPlayerInterior(user), GetPlayerVirtualWorld(user));
		}
		case 3: {
			SetPlayerPosEx(user, 5, 223.1916, 109.9863, 999.0156, 0, GetPlayerInterior(user), GetPlayerVirtualWorld(user));
		}
		default: {
			SetPlayerPosEx(user, 5, 227.1033, 109.8796, 999.0156, 0, GetPlayerInterior(user), GetPlayerVirtualWorld(user));
		}
	}
	
	SetCameraBehindPlayer(user);
	
	format(msg, sizeof(msg), "You have been arrested by the Los Angeles Police Department for %i minutes.", time);
	SendClientMessage(user, COLOR_WHITE, msg);
	format(string, sizeof(string), "LAPD: %s has been sent to Los Angeles County Jail %s for %i minutes.", PlayerInfo[user][pName], PlayerInfo[playerid][pName], time);
	SendClientMessageToAll(COLOR_LIGHTRED, string);
	SetPVarInt(user, "Arrest", time);
	SetPVarInt(user, "Arrested", 1);
	mysql_format(sqlHandle, msg, sizeof(msg), "INSERT INTO `samp_arrests` (`user`, `time`, `reason`) VALUES ('%e', %i, '%e')", PlayerInfo[user][pName], time, reason);
	mysql_function_query(sqlHandle, msg, false, "", "");
	return 1;
}

