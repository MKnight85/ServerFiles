COMMAND:revive(playerid, params[])
{
	if(PlayerInfo[playerid][pFaction] == 2)
	{
	    new user;
	    if(sscanf(params, "u", user)) return SendClientMessage(playerid, COLOR_GREY, "USAGE: /revive [playerid]");
	    if(user == INVALID_PLAYER_ID) sscanf(params, "i", user);
 		if(user > MAX_PLAYERS) user = DeterminePlayerID(user);
	    if(!IsPlayerConnected(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID is not connected).");
	    if(IsPlayerNPC(user)) return SendClientMessage(playerid, COLOR_GREY, "Invalid playerID (Submitted playerID points at NPC).");
	    if(GetPVarInt(user, "Death") != 1) return SendClientMessage(playerid, COLOR_GREY, "User is not critically injured.");
		if(playerid == user) return SendClientMessage(playerid, COLOR_GREY, "You cannot revive yourself.");
		new msg[86], Float:X, Float:Y, Float:Z;
		GetPlayerPos(user, X, Y, Z);
	    if(!IsPlayerInRangeOfPoint(playerid, 5, X, Y, Z)) return SendClientMessage(playerid, COLOR_GREY, "You are not around this player.");
		SetPVarInt(user, "Death", 0);
		format(msg, sizeof(msg), "** %s uses %s shock paddles to revive %s.", GetNameWithSpace(playerid, 1), HisHer(playerid), GetNameWithSpace(user, 1));
		ProxDetector(12, playerid, msg, COLOR_PINK);
		StopTalkVar[playerid] = SetTimerEx("StopTalk", 100, false, "d", user);
		SetPlayerHealthAndLimit(user, 20);
		PlayerInfo[playerid][pHealth] = 20;
		SetCameraBehindPlayer(user);
		UnfreezePlayer(user);
		Delete3DTextLabel(DeathReason[user]);
	}
	else
	{
	    SendClientMessage(playerid, COLOR_LIGHTRED, "You are not authorized to use this command.");
	}
	return 1;
}

